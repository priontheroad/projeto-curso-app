isLoading = payload\n        },\n        setIsLoadingTributos(state, payload) {\n            state.isLoadingTributos = payload\n        },\n        setAntidumping(state, payload) {\n            state.ANTIDUMPING = payload;\n        },\n        setCide(state, payload) {\n            state.CIDE = payload;\n        },\n        setIcms(state, payload) {\n            state.ICMS = payload;\n        },\n        setTipoIcms(state, payload) {\n            state.ICMS.TIPO_TRIBUTACAO = payload;\n        },\n        setII(state, payload) {\n            state.II = payload;\n        },\n        setIpi(state, payload) {\n            state.IPI = payload;\n        },\n        setPisCofins(state, payload) {\n            state.PIS_COFINS = payload;\n        }\n    },\n    actions:{\n        obterAlertaAntidumping({commit}, dados){\n            commit(\"setIsLoading\",true);\n\n            return new Api().post(\"/aliquotasNcm/antidumping\", dados)\n            .then((response) => {\n                commit('setAntidumping', response.data.ANTIDUMPING);\n                commit(\"setIsLoading\",false);\n            })\n            .catch(() => {\n                commit('setAntidumping', []);\n                commit(\"setIsLoading\",false);\n            });\n        },\n        obterAliquotas({commit}, dados){\n            commit(\"setIsLoadingTributos\",true);\n            \n            return new Api().post(\"/aliquotasNcm\", dados)\n            .then((response) => {\n                commit('setCide', response.data.CIDE);\n                commit('setIcms', response.data.ICMS);\n                commit('setII', response.data.II);\n                commit('setIpi', response.data.IPI);\n                commit('setPisCofins', response.data.PIS_COFINS);\n                commit(\"setIsLoadingTributos\",false);\n            })\n            .catch(() => {\n                commit('setCide', []);\n                commit('setIcms', []);\n                commit('setII', []);\n                commit('setIpi', []);\n                commit('setPisCofins', []);\n                commit(\"setIsLoadingTributos\",false);\n            });\n        },\n        obterAliquotasIcms({commit}, dados){\n            commit(\"setIsLoadingTributos\",true);\n\n            return new Api().post(\"/aliquotasNcm/icms\", dados)\n            .then((response) => {\n                commit('setIcms', response.data.ICMS);\n                commit(\"setIsLoadingTributos\",false);\n            })\n            .catch(() => {\n                commit('setIcms', []);\n                commit(\"setIsLoadingTributos\",false);\n            });\n        },\n        limparAliquotasEAlertas({commit}) {\n            commit('setAntidumping', []);\n            commit('setCide', []);\n            commit('setIcms', []);\n            commit('setII', []);\n            commit('setIpi', []);\n            commit('setPisCofins', []);\n            commit(\"setIsLoading\",false);\n            commit(\"setIsLoadingTributos\",false);\n        },\n        limparAliquotasEAlertasIcms({commit}) {\n            commit('setIcms', []);\n            commit(\"setIsLoading\",false);\n            commit(\"setIsLoadingTributos\",false);\n        }\n    }\n}"]}]}