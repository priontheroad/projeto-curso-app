   getState: (state) => {\n            return state;\n        },\n        getStatusCarregamento: (state) => {\n            return state.status_carregamento;\n        },\n        getEstados: (state) => {\n            return state.estados;\n        },\n        getIncoterm: (state) => {\n            return state.incoterm;\n        },\n        getTipoTributacao: (state) => {\n            return state.tipo_tributacao;\n        },\n        getTipoMoeda: (state) => {\n            return state.tipo_moeda;\n        },\n        getUnidadeMedida: (state) => {\n            return state.unidade_medida;\n        },\n        getUnidadeMedidaAgrupada: (state) => {\n            let idTipoMedida = null;\n            let unidades = [];\n\n            state.unidade_medida.forEach(um => {\n                if (idTipoMedida != um.id_tipo_unidade_medida) {\n                    unidades.push({\n                        \"header\": um.tipo,\n                    });\n                    idTipoMedida = um.id_tipo_unidade_medida;\n                }\n                unidades.push(um);\n            });\n\n            return unidades;\n        },\n        getTipoOperacao: (state) => {\n            return state.tipo_operacao;\n        },\n        getPaises: (state) => {\n            return state.paises\n        }\n    },\n    mutations: {\n        setStatusCarregamento: (state, payload) => {\n            state.status_carregamento = payload;\n        },\n        setEstados: (state, estados) => {\n            state.estados = estados;\n        },\n        setIncoterm: (state, incoterm) => {\n            state.incoterm = incoterm;\n        },\n        setTipoTributacao: (state, tipo_tributacao) => {\n            state.tipo_tributacao = tipo_tributacao;\n        },\n        setTipoMoeda: (state, tipo_moeda) => {\n            state.tipo_moeda = tipo_moeda;\n        },\n        setUnidadeMedida: (state, unidade_medida) => {\n            state.unidade_medida = unidade_medida;\n        },\n        setTipoOperacao: (state, tipo_operacao) => {\n            state.tipo_operacao = tipo_operacao;\n        },\n        setPaises: (state, paises) => {\n            state.paises = paises;\n        }\n\n    },\n    actions: {\n        carregaInformacoes({ commit }) {\n            new Api().get('/informacoes')\n            .then(async response => {\n                if (typeof response !== 'undefined' && response.status == '200') {\n                    if (typeof response !== 'undefined' && typeof response.data !== 'undefined') {\n                        response = response.data;\n                        commit('setStatusCarregamento', true);\n                        commit('setEstados', response.estados);\n                        commit('setIncoterm', response.incoterm);\n                        commit('setPaises', response.paises);\n                        commit('setTipoMoeda', response.tipo_moeda);\n                        commit('setTipoTributacao', response.tipo_tributacao);\n                        commit('setUnidadeMedida', response.unidade_medida);\n                        commit('setTipoOperacao', response.tipo_operacao);\n                    }\n                }else{\n                    commit('setStatusCarregamento', false);\n                    Object.bus.$emit(\"structure\", {\n                        message: \"Erro ao carregar informações, entre em contato com a econet.\",\n                        type: \"error\",\n                        timeout: 10000,\n                    });\n                }\n            }).catch(() => {\n                commit('setStatusCarregamento', false);\n                Object.bus.$emit(\"structure\", {\n                    message: \"Erro ao carregar informações, entre em contato com a econet.\",\n                    type: \"error\",\n                    timeout: 10000,\n                });\n          });\n        }\n    }\n}"]}]}