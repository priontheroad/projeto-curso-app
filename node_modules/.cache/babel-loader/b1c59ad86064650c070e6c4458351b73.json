CAAA,CAAA;AACA,KATA;AAWA,IAAA,gBAAA,EAAA,4BAAA;AACA,UAAA,CAAA,KAAA,SAAA,CAAA,IAAA,EAAA;AACA,eAAA,EAAA;AACA;;AACA,UAAA,UAAA,GAAA,KAAA,SAAA,CAAA,IAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AACA,aAAA,KAAA,QAAA,CAAA,UAAA,CAAA,CAAA,CAAA,CAAA;AACA;AAjBA;AALA,CAAA","sourcesContent":["<template>\n  <div class=\"d-flex justify-space-between my-4 px-10\">\n    <div>\n      <span class=\"text-caption font-weight-bold\">Nº DI/DUIMP</span>\n      <p>{{ resultado.numero_declaracao }}</p>\n    </div>\n    <div>\n      <span class=\"text-caption font-weight-bold\">Data Declaração</span>\n      <p>{{ dateTimeComputed }}</p>\n    </div>\n    <div>\n      <span class=\"text-caption font-weight-bold\">UF</span>\n      <p>{{ resultado.estado.sigla }}</p>\n    </div>\n    <div>\n      <span class=\"text-caption font-weight-bold\">Modal de Operação</span>\n      <p>{{ resultado.tipo_operacao.descricao }}</p>\n    </div>\n    <v-divider vertical class=\"mb-3 color-divider\"></v-divider>\n    <div>\n      <span class=\"text-caption font-weight-bold\">Adições</span>\n      <p>{{ resultado.qtd_adicoes }}</p>\n    </div>\n    <div>\n      <span class=\"text-caption font-weight-bold\">Peso Líq. Total</span>\n      <p>{{ unidadeMedidaFormat }}</p>\n    </div>\n    <v-divider vertical class=\"mb-3 color-divider\"></v-divider>\n    <div>\n      <span class=\"text-caption font-weight-bold\">Taxa Siscomex</span>\n      <p>{{ vlrMoeda(resultado.dados_gerais.taxa_siscomex, \"BRL\") }}</p>\n    </div>\n    <div>\n      <span class=\"text-caption font-weight-bold text-right\">Valor Aduaneiro (VMLD)</span>\n      <p class=\"formata-color-azul font-weight-bold text-right\">{{ vlrMoeda(resultado.dados_gerais.valor_aduaneiro, \"BRL\") }}</p>\n    </div>\n  </div>\n</template>\n\n<script>\n  import { mapGetters } from 'vuex';\n  import formataDataMixin from \"@/mixins/formataDataMixin\";\n  import formataMoedaMixin from \"@/mixins/formataMoedaMixin\";\n  import formataUnidadeMedidaMixin from \"@/mixins/formataUnidadeMedidaMixin\";\n\n  export default {\n    name: 'QuadroResumo',\n\n    mixins: [formataDataMixin, formataMoedaMixin, formataUnidadeMedidaMixin],\n\n    computed: {\n      ...mapGetters('resultadosFinal', { resultado: 'getState', }),\n\n      unidadeMedidaFormat: function () {\n        if (!this.resultado.mercadoria.peso_liquido_total) {\n          return '';\n        }\n        const unidadeMedida = this.resultado.mercadoria.peso_liquido_total;\n        return this.unidadeKG(parseFloat(unidadeMedida).toFixed(4));\n      },\n\n      dateTimeComputed: function () {\n        if (!this.resultado.data) {\n          return '';\n        }\n        const arDateTime = this.resultado.data.split(\" \");\n        return this.datePtBR(arDateTime[0]);\n      },\n\n    }\n  }\n</script>\n\n<style lang=\"scss\">\n  .quadro-resumo {\n    height: 80px;\n  }\n</style>"],"sourceRoot":"src/components/QuadrosResultado"}]}