AUQ,KAAV,EAAiB;AACf,UAAMT,KAAK,GAAG,KAAKE,QAAL,EAAd;AACA,UAAMC,IAAI,GAAG,KAAKC,aAAL,GAAqBC,OAArB,CAA6B,WAA7B,EAA0C,EAA1C,CAAb;AACA,aAAO3B,KAAK,CAACgC,IAAN,CACLP,IAAI,GAAGF,GADF,EAELQ,KAFK,EAEE;AACPd,QAAAA,OAAO,EAAE;AACP,0BAAgB,qBADT;AAEP,2BAAiB,YAAYK;AAFtB;AADF,OAFF,EAQLO,IARK,CASL,UAAC1B,QAAD,EAAc;AACZ,eAAOA,QAAQ,CAACgC,IAAhB;AACD,OAXI,EAYLL,KAZK,CAaL,UAACzB,KAAD,EAAW;AACT,eAAOA,KAAK,CAACF,QAAb;AACD,OAfI,CAAP;AAiBD;;;WAED,yBAAgB;AACd,UAAI,OAAOmC,OAAO,CAACC,GAAR,CAAYC,gBAAnB,KAAwC,WAA5C,EAAyD;AAEvD,YAAIF,OAAO,CAACC,GAAR,CAAYC,gBAAZ,IAAgC,OAApC,EAA6C;AAC3C,iBAAOF,OAAO,CAACC,GAAR,CAAYE,sBAAnB;AACD;;AAED,YAAIH,OAAO,CAACC,GAAR,CAAYC,gBAAZ,IAAgC,MAApC,EAA4C;AAC1C,iBAAOF,OAAO,CAACC,GAAR,CAAYG,qBAAnB;AACD;AACF,OATD,MASO;AACL,YAAMnB,GAAG,GAAGjB,MAAM,CAACC,QAAP,CAAgBoC,IAAhB,CAAqBC,QAArB,EAAZ;;AAEA,YAAIrB,GAAG,CAACd,OAAJ,CAAY,kBAAZ,KAAmC,CAAvC,EAA0C;AAExC,iBAAO6B,OAAO,CAACC,GAAR,CAAYE,sBAAnB;AAED;;AAED,YAAIlB,GAAG,CAACd,OAAJ,CAAY,+CAAZ,KAAgE,CAApE,EAAuE;AACrE,iBAAO6B,OAAO,CAACC,GAAR,CAAYG,qBAAnB;AACD;;AAED,eAAOJ,OAAO,CAACC,GAAR,CAAYG,qBAAnB;AACD;AACF;;;;;;SAnNkBvB,G","sourcesContent":["import axios from 'axios'\nimport store from '@/store'\n\n// Add a response interceptor\naxios.interceptors.response.use(function (response) {\n  return response\n}, function (error) {\n  if (window.location.pathname !== '/erro/401' && window.location.pathname !== '/erro/404' && window.location.pathname.indexOf('/auth') === -1) {\n    if (typeof error.response !== 'undefined') {\n      switch (parseInt(error.response.status)) {\n        case 400:\n          return error.response;\n        case 401:\n          window.location = '/autenticar'\n          break;\n        case 404:\n          window.location = '/erro/404'\n          break\n        case 422:\n          //continua e passa erro\n          return error.response\n        default:\n          Promise.reject(error)\n          return {\n            response: {}\n          }\n      }\n    }\n  }\n})\n// comunicação para retorno dados tabela modal resultados e pagina inicial\nexport const http = axios.create({\n  baseURL: `https://rateioimportacao-api.econeteditora.com.br/api`,\n  headers: {\n    'Content-Type': 'application/json;',\n    Accept: 'application/json'\n  }\n});\n\nexport default class Api {\n\n  getToken() {\n    return store.state.auth.token\n  }\n\n  get(url, auth = true) {\n    const token = this.getToken();\n\n    auth = auth == true ? \"Bearer \" + token : \"\";\n\n    const conn = this.getConnection().replace('undefined', '')\n    return axios.get(conn + url, {\n      headers: {\n        'Content-Type': 'application/json',\n        Accept: 'application/json',\n        \"Authorization\": auth\n      }\n    }).then(\n      (response) => {\n        // return response.data\n        return response\n      }\n    ).catch(\n      (error) => {\n        // return error.response\n        return error\n      }\n    )\n  }\n\n  post(url, dados) {\n    const token = this.getToken();\n\n    const conn = this.getConnection().replace('undefined', '')\n    return axios.post(conn + url, {\n      ...dados\n    }, {\n      headers: {\n        'Content-Type': 'application/json',\n        Accept: 'application/json',\n        'Authorization': 'Bearer ' + token\n      }\n    }).then(\n      (response) => {\n        return response\n      }\n    ).catch(\n      (error) => {\n        return error\n      }\n    )\n  }\n  postFileXML(url, dados) {\n    const token = this.getToken();\n\n    const conn = this.getConnection().replace('undefined', '')\n    return axios.post(conn + url, \n      dados\n    , {\n      headers: {\n        'Content-Type': 'multipart/form-data',\n        Accept: 'application/xml',\n        'Authorization': 'Bearer ' + token\n      }\n    }).then(\n      (response) => {\n        return response\n      }\n    ).catch(\n      (error) => {\n        return error\n      }\n    )\n  }\n\n  postArray(url, dados) {\n    const token = this.getToken();\n\n    const conn = this.getConnection().replace('undefined', '')\n    return axios.post(conn + url, [\n      ...dados\n    ], {\n      headers: {\n        'Content-Type': 'application/json',\n        Accept: 'application/json',\n        'Authorization': 'Bearer ' + token\n      }\n    }).then(\n      (response) => {\n        return response\n      }\n    ).catch(\n      (error) => {\n        return error\n      }\n    )\n  }\n\n  put(url, dados) {\n    const token = this.getToken();\n\n    const conn = this.getConnection().replace('undefined', '')\n    return axios.put(conn + url, {\n      ...dados\n    }, {\n      headers: {\n        'Content-Type': 'application/json',\n        Accept: 'application/json',\n        'Authorization': 'Bearer ' + token\n      }\n    }).then((response) => {\n      return response\n    }\n    ).catch((error) => {\n      return error\n    }\n    )\n  }\n\n  putArray(url, dados) {\n    const token = this.getToken();\n\n    const conn = this.getConnection().replace('undefined', '')\n    return axios.put(conn + url, [\n      ...dados\n    ], {\n      headers: {\n        'Content-Type': 'application/json',\n        Accept: 'application/json',\n        'Authorization': 'Bearer ' + token\n      }\n    }).then((response) => {\n      return response\n    }\n    ).catch((error) => {\n      return error\n    }\n    )\n  }\n\n\n  delete(url, dados) {\n    const token = this.getToken();\n\n    const conn = this.getConnection().replace('undefined', '')\n    return axios.delete(conn + url, {\n      headers: {\n        'Content-Type': 'application/json',\n        Accept: 'application/json',\n        'Authorization': 'Bearer ' + token\n      },\n      data: Object.assign({}, dados)\n    }).then(\n      (response) => {\n        return response\n      }\n    ).catch(\n      (error) => {\n        return error\n      }\n    )\n  }\n\n  file(url, dados) {\n    const token = this.getToken()\n    const conn = this.getConnection().replace('undefined', '')\n    return axios.post(\n      conn + url,\n      dados, {\n      headers: {\n        'Content-Type': 'multipart/form-data',\n        'Authorization': 'Bearer ' + token\n      }\n    }\n    ).then(\n      (response) => {\n        return response.data\n      }\n    ).catch(\n      (error) => {\n        return error.response\n      }\n    )\n  }\n\n  getConnection() {\n    if (typeof process.env.VUE_APP_AMBIENTE !== 'undefined') {\n\n      if (process.env.VUE_APP_AMBIENTE == 'LOCAL') {\n        return process.env.VUE_APP_ROOT_API_LOCAL;\n      }\n\n      if (process.env.VUE_APP_AMBIENTE == 'PROD') {\n        return process.env.VUE_APP_ROOT_API_PROD;\n      }\n    } else {\n      const url = window.location.href.toString()\n\n      if (url.indexOf('http://localhost') >= 0) {\n\n        return process.env.VUE_APP_ROOT_API_LOCAL\n\n      }\n\n      if (url.indexOf('https://rateioimportacao.econeteditora.com.br') >= 0) {\n        return process.env.VUE_APP_ROOT_API_PROD\n      }\n\n      return process.env.VUE_APP_ROOT_API_PROD\n    }\n  }\n\n}\n"]}]}