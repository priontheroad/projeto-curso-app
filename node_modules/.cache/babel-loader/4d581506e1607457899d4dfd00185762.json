,IAAA,iBAAA,EAAA;AACA,MAAA,GADA,iBACA;AACA,eAAA,CACA;AAAA,UAAA,IAAA,EAAA,GAAA;AAAA,UAAA,KAAA,EAAA,OAAA;AAAA,UAAA,QAAA,EAAA,KAAA;AAAA,UAAA,KAAA,EAAA;AAAA,SADA,EAEA;AAAA,UAAA,IAAA,EAAA,mBAAA;AAAA,UAAA,KAAA,EAAA,gBAAA;AAAA,UAAA,QAAA,EAAA;AAAA,SAFA,EAGA;AAAA,UAAA,IAAA,EAAA,SAAA;AAAA,UAAA,QAAA,EAAA,KAAA;AAAA,UAAA,KAAA,EAAA;AAAA,SAHA,EAIA;AAAA,UAAA,IAAA,EAAA,eAAA;AAAA,UAAA,QAAA,EAAA,KAAA;AAAA,UAAA,KAAA,EAAA;AAAA,SAJA,EAKA;AAAA,UAAA,IAAA,EAAA,oBAAA;AAAA,UAAA,QAAA,EAAA,KAAA;AAAA,UAAA,KAAA,EAAA;AAAA,SALA,EAMA;AAAA,UAAA,IAAA,EAAA,sBAAA;AAAA,UAAA,QAAA,EAAA,KAAA;AAAA,UAAA,KAAA,EAAA;AAAA,SANA,EAOA;AAAA,UAAA,IAAA,EAAA,0CAAA;AAAA,UAAA,QAAA,EAAA,KAAA;AAAA,UAAA,KAAA,EAAA;AAAA,SAPA,EAQA;AAAA,UAAA,IAAA,EAAA,aAAA;AAAA,UAAA,QAAA,EAAA,KAAA;AAAA,UAAA,KAAA,EAAA;AAAA,SARA,CAAA;AAUA,OAZA;AAaA,MAAA,GAbA,eAaA,CAbA,EAaA;AACA,eAAA,CAAA;AACA;AAfA,KADA;AAmBA,IAAA,kBAAA,EAAA;AACA,MAAA,GADA,iBACA;AACA,eAAA,KAAA,MAAA,CAAA,KAAA,CAAA,eAAA,CAAA,kBAAA;AACA,OAHA;AAIA,MAAA,GAJA,eAIA,QAJA,EAIA;AACA,YAAA,QAAA,IAAA,KAAA,kBAAA,EAAA;AACA,eAAA,MAAA,CAAA,MAAA,CAAA,sCAAA,EAAA,QAAA;AACA;AACA;AARA,KAnBA;AA8BA,IAAA,yBAAA,EAAA;AACA,MAAA,GADA,iBACA;AACA,eAAA,KAAA,MAAA,CAAA,KAAA,CAAA,eAAA,CAAA,yBAAA;AACA,OAHA;AAIA,MAAA,GAJA,eAIA,QAJA,EAIA;AACA,YAAA,QAAA,IAAA,KAAA,yBAAA,EAAA;AACA,eAAA,MAAA,CAAA,MAAA,CACA,4CADA,EAEA,QAFA;AAIA;AACA;AAXA;AA9BA;AApBA,CAAA","sourcesContent":["<template>\n  <v-card flat class=\"mx-auto mt-4 format-border-card\" max-width=\"100%\">\n    <v-card-title>\n      <span style=\"color: #2d5071; font-size: 16px; font-weight: 800\">Descrição Detalhada da Mercadoria</span>\n    </v-card-title>\n    <v-card-text>\n      <v-data-table\n        :headers=\"headersMercadoria\"\n        :items=\"detalhe_mercadoria\"\n        item-key=\"id\"\n        :items-per-page=5\n        :footer-props=\"{\n          showFirstLastPage: false\n        }\"\n        dense\n      >\n        <template v-slot:item=\"{ item }\">\n          <tr>\n            <td>{{ item.id_gerado }}</td>\n            <td>{{ item.descricao | truncate(27, \"...\") }}</td>\n            <td>{{ vlrDecimal(item.quantidade) }}</td>\n            <td>{{ vlrDecimal(item.peso_unitario) }} {{ item.unidade_medida.sigla_unidade }}</td>\n            <td>{{ vlrDecimal(item.peso_total_item) }} KG</td>\n            <td>{{ vlrMoeda(item.vucv_real, \"BRL\") }}</td>\n            <td>{{ vlrMoeda(item.vmcv_real, \"BRL\") }}</td>\n            <td>{{ vlrMoeda(item.antidumping, \"BRL\") }}</td>\n          </tr>\n        </template>\n\n        <template slot=\"body.append\">\n          <tr>\n            <th class=\"text-caption font-weight-bold\" style=\"padding-left: 0\">Total</th>\n            <th></th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">\n              {{ vlrDecimal(totais_detalhe_mercadoria.quantidade) }}\n            </th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">\n              <!-- Tiraram o casaco dia 10/11 a pedido da Ingrid -->\n              <!-- {{ vlrDecimal(totais_detalhe_mercadoria.peso_unitario) }} -->\n            </th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">\n              {{ vlrDecimal(totais_detalhe_mercadoria.peso_liquido_total) }}\n            </th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">\n              <!-- {{ vlrMoeda(totais_detalhe_mercadoria.vucv, \"BRL\") }} -->\n            </th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">\n              {{ vlrMoeda(totais_detalhe_mercadoria.vmcv, \"BRL\") }}\n            </th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">\n              {{ vlrMoeda(totais_detalhe_mercadoria.antidumping, \"BRL\") }}\n            </th>\n          </tr>\n        </template>\n      </v-data-table>\n    </v-card-text>\n  </v-card>\n</template>\n<script>\n  import formataMoedaMixin from \"@/mixins/formataMoedaMixin\";\n\n  export default {\n    name: 'QuadroDescricaoDetalhadaMercadoria',\n\n    mixins: [formataMoedaMixin],\n    \n    filters: {\n      truncate: function (text, length, suffix) {\n        if (\n          typeof text !== \"undefined\" &&\n          text !== null &&\n          text !== \"\" &&\n          text.length > length\n        ) {\n          return text.substring(0, length) + suffix;\n        } else {\n          return text;\n        }\n      },\n    },\n\n    computed: {\n      headersMercadoria: {\n        get() {\n          return [\n            { text: \"#\", align: \"start\", sortable: false, value: \"id\", },\n            { text: \"Descrição do Item\", value: \"descricao_item\", sortable: false },\n            { text: \"Quantid\", sortable: false, value: \"quantidade_mercadoria\", },\n            { text: \"Peso Unitário\", sortable: false, value: \"peso_unitario\", },\n            { text: \"Peso Líquido Total\", sortable: false, value: \"peso_total_item\", },\n            { text: \"Valor Unitário(VUMC)\", sortable: false, value: \"vumc\", },\n            { text: \"Valor Mercadoria Condição de venda(VMCV)\", sortable: false, value: \"vmcv\", },\n            { text: \"Antidumping\", sortable: false, value: \"antidumping\" },\n          ];\n        },\n        set(h) {\n          return h;\n        }\n      },\n\n      detalhe_mercadoria: {\n        get() {\n          return this.$store.state.resultadosFinal.detalhe_mercadoria;\n        },\n        set(newValue) {\n          if (newValue != this.detalhe_mercadoria) {\n            this.$store.commit(\"resultadosFinal/setDetalheMercadoria\", newValue);\n          }\n        },\n      },\n\n      totais_detalhe_mercadoria: {\n        get() {\n          return this.$store.state.resultadosFinal.totais_detalhe_mercadoria;\n        },\n        set(newValue) {\n          if (newValue != this.totais_detalhe_mercadoria) {\n            this.$store.commit(\n              \"resultadosFinal/setTotaisDetalheMercadoria\",\n              newValue\n            );\n          }\n        },\n      },\n    },\n  }\n</script>"],"sourceRoot":"src/components/QuadrosResultado"}]}