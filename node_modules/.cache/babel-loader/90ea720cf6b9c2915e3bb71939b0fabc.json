;AACA,UAAIc,KAAK,CAACE,MAAN,KAAiB,CAArB,EAAwB;AACpB,eAAOF,KAAK,CAACG,GAAN,GAAYF,KAAZ,CAAkB,GAAlB,EAAuBG,KAAvB,EAAP;AACH;AACF;;;WACD,sBAAalB,IAAb,EAAmBmB,IAAnB,EAAyBP,KAAzB,EAAgC;AAC9B,UAAG,OAAOA,KAAP,IAAgB,WAAhB,IAA+BA,KAAK,IAAI,EAA3C,EAA8C;AAC5CA,QAAAA,KAAK,GAAG7B,OAAO,CAACC,GAAR,CAAYC,oBAApB;AACD;;AACD,UAAImC,OAAO,GAAG,EAAd;;AACA,UAAID,IAAJ,EAAU;AACR,YAAIE,IAAI,GAAG,IAAIC,IAAJ,EAAX;AACAD,QAAAA,IAAI,CAACE,OAAL,CAAaF,IAAI,CAACG,OAAL,KAAkBL,IAAI,GAAC,EAAL,GAAQ,EAAR,GAAW,EAAX,GAAc,IAA7C;AACAC,QAAAA,OAAO,GAAG,eAAeC,IAAI,CAACI,WAAL,EAAzB;AACD;;AACDZ,MAAAA,QAAQ,CAAC1B,MAAT,GAAkBa,IAAI,GAAG,GAAP,IAAcY,KAAK,IAAI,EAAvB,IAA8BQ,OAA9B,GAAwC,UAA1D;AACD;;;WAED,wBAAc;AACZ,UAAIX,KAAK,GAAG,qBAAZ;AACAiB,MAAAA,KAAK,CAACjB,KAAD,CAAL;AACAZ,MAAAA,YAAY,CAAC8B,KAAb;AAEA/C,MAAAA,MAAM,CAACC,QAAP,GAAkB,WAAlB;AACD;;;;;;SA3FgBH,I","sourcesContent":["import Api from '../../services/api'\nimport store from '@/store'\nimport router from '@/router/router'\n\nexport default class Auth {\n    constructor() {\n\n    }\n\n    generateToken(){\n      let host = window.location.host;\n\n      //se for localhost cria cookie valido\n      if(\n        host.indexOf('localhost') >= 0 &&\n        typeof process.env.VUE_APP_VALUE_COOKIE !== 'undefined' &&\n        process.env.VUE_APP_VALUE_COOKIE !== \"\"\n      ){\n        this.createCookie('bG9naW4', 1);\n      }\n\n      let cookie = this.getCookie(\"bG9naW4\");\n\n      if(typeof cookie == 'undefined'){\n        this.redirecionar();\n        return false;\n      }\n      cookie = cookie.replaceAll(\"%3D\", \"\");\n      return this.verifyUserAndGetToken(cookie);\n    }\n  \n    async verifyUserAndGetToken(cookie){\n        return new Api().get('/logar/'+ cookie, false)\n        .then(response => {\n          if(typeof response !== 'undefined' && (response.status == '200' || response.status == '201') ){\n            if(typeof response.data !== 'undefined'){\n              localStorage.setItem('token', response.data.token);\n              localStorage.setItem('nome_usuario', response.data.name);\n              localStorage.setItem('id_usuario', response.data.id);\n              localStorage.setItem('email_usuario', response.data.email);\n  \n              //adicionando no vuex\n              store.commit('auth/setToken', response.data.token)\n              store.commit('auth/setNomeUsuario', response.data.name)\n              store.commit('auth/setIdUsuario', response.data.id)\n              store.commit('auth/setEmailUsuario', response.data.email)\n\n              if(!(router.currentRoute.path == '/')){\n                router.push({ path: '/'});\n              }else{\n                window.location.reload(true);\n              }\n              return true;\n\n            }else{\n              this.redirecionar();\n              return false;\n            }\n          }else{\n            this.redirecionar();\n            return false;\n          }\n        }).catch(((error) => {\n          console.log(error);\n          this.redirecionar();\n          return false;\n        }));\n    }\n  \n    getCookie(name) {\n      const value = `; ${document.cookie}`;\n      const parts = value.split(`; ${name}=`);\n      if (parts.length === 2) {\n          return parts.pop().split(';').shift();\n      }\n    }\n    createCookie(name, days, value) {\n      if(typeof value == 'undefined' || value == \"\"){\n        value = process.env.VUE_APP_VALUE_COOKIE;\n      }\n      var expires = \"\";\n      if (days) {\n        var date = new Date();\n        date.setTime(date.getTime() + (days*24*60*60*1000));\n        expires = \"; expires=\" + date.toUTCString();\n      }\n      document.cookie = name + \"=\" + (value || \"\")  + expires + \"; path=/\";\n    }\n\n    redirecionar(){\n      let error = 'Usúario não logado.';\n      alert(error);\n      localStorage.clear();\n\n      window.location = '/erro/401';\n    }\n\n}"]}]}