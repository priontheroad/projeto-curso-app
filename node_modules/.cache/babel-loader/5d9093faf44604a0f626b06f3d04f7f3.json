ey: {\n        decimal: ',',\n        thousands: '.',\n        prefix: '',\n        suffix: '',\n        precision: 2,\n        masked: false\n      }\n    }\n  },\n\n  created() {\n    this.valorMonetario = this.value;\n    this.inicio = false;\n\n    if (this.blPorcentagem) {\n      this.prependIcon = '';\n      this.suffixIcon = ' %';\n    }\n\n    if (this.appendIcon != '') {\n      this.suffixIcon = this.appendIcon;\n      this.prependIcon = '';\n    }\n\n    this.money.precision = this.intDecimais\n  },\n\n  watch: {\n    valorMonetario() {\n      if (this.inicio) {\n        return;\n      }\n      this.$emit('input', this.convertePadraoBase(this.valorMonetario));\n    },\n    value(newValue, oldValue) {\n      if (this.inicio) {\n        return;\n      }\n      if (this.inicio) {\n        this.valorMonetario = oldValue;\n        this.inicio = false;\n        return;\n      }\n    }\n  },\n}\n</script>"],"sourceRoot":"src/components/Formularios"}]}