A;AACA,KA/CA;AAiDA,IAAA,cAjDA,4BAiDA;AACA,UAAA,cAAA,GAAA,KAAA;AACA,WAAA,SAAA,CAAA,OAAA,CAAA,UAAA,EAAA,EAAA;AACA,YAAA,EAAA,CAAA,WAAA,CAAA,qBAAA,CAAA,QAAA,IAAA,IAAA,IAAA,EAAA,CAAA,WAAA,CAAA,IAAA,CAAA,kBAAA,IAAA,CAAA,EAAA;AACA,UAAA,cAAA,GAAA,IAAA;AACA;AACA,OAJA;AAKA,aAAA,cAAA,IAAA,KAAA,IAAA,CAAA,WAAA,CAAA,cAAA,GAAA,CAAA;AACA,KAzDA;AA2DA,IAAA,WA3DA,yBA2DA;AACA,UAAA,WAAA,GAAA,KAAA;AACA,WAAA,SAAA,CAAA,OAAA,CAAA,UAAA,EAAA,EAAA;AACA,YAAA,EAAA,CAAA,WAAA,CAAA,iBAAA,CAAA,KAAA,GAAA,CAAA,EAAA;AACA,UAAA,WAAA,GAAA,IAAA;AACA;AACA,OAJA;AAKA,aAAA,WAAA;AACA;AAnEA;AATA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-row no-gutters>\n      <v-col cols=\"8\">\n        <span class=\"text-caption font-weight-bold\">Tributos</span>\n      </v-col>\n      <v-col cols=\"4\" align=\"end\">\n        <span class=\"text-caption font-weight-bold\">Total</span>\n      </v-col>\n    </v-row>\n\n    <v-divider class=\"mt-2 mb-2\"></v-divider>\n\n    <div v-for=\"(t, index) in tributos\" :key=\"index\">\n      <v-row no-gutters v-if=\"t.blExibe\">\n        <v-col cols=\"8\">\n          <span class=\"text-caption font-weight-regular\">{{ t.tributo }}</span>\n        </v-col>\n        <v-col cols=\"4\" align=\"end\">\n          <span class=\"text-caption font-weight-regular informativos-tela\">{{ t.valor }}</span>\n        </v-col>\n      </v-row>\n    </div>\n\n    <v-divider class=\"mt-2 mb-2\"></v-divider>\n    \n    <v-row>\n      <v-col cols=\"8\">\n        <span class=\"text-caption font-weight-bold\">Total de Tributos</span>\n      </v-col>\n      <v-col cols=\"4\" align=\"end\">\n        <span class=\"text-caption font-weight-bold formata-color-azul\">\n          {{ vlrMoeda(item.totais.total_tributos, \"BRL\") }}\n        </span>\n      </v-col>\n    </v-row>\n  </div>\n</template>\n<script>\n  import formataMoedaMixin from \"@/mixins/formataMoedaMixin\";\n  import { mapGetters } from 'vuex';\n\n  export default {\n    name: 'QuadroTributosNF',\n    \n    mixins: [formataMoedaMixin],\n\n    props: {\n      item: { required: true }\n    },\n\n    computed: {\n      ...mapGetters(\"resultadosFinal\", { resultado: \"getState\" }),\n\n      tributos() {\n        return [\n          { tributo: 'II', valor: this.vlrMoeda(this.item.tributacoes.ii_total, \"BRL\"), blExibe: true },\n          { tributo: 'IPI', valor: this.vlrMoeda(this.item.tributacoes.ipi_total, \"BRL\"), blExibe: true },\n          { tributo: 'PIS - Importação', valor: this.vlrMoeda(this.item.tributacoes.pis_total, \"BRL\"), blExibe: true },\n          { tributo: 'COFINS - Importação', valor: this.vlrMoeda(this.item.tributacoes.cofins_total, \"BRL\"), blExibe: true },\n          { tributo: 'ICMS', valor: this.vlrMoeda(this.item.tributacoes.icms_total, \"BRL\"), blExibe: true },\n          { tributo: 'ICMS-ST', valor: this.vlrMoeda(this.item.tributacoes.icms_st_total, \"BRL\"), blExibe: this.blExibeIcmsSt },\n          { tributo: 'FUNDO', valor: this.vlrMoeda(this.item.tributacoes.fundo_total, \"BRL\"), blExibe: this.blExibeFundo },\n          { tributo: 'FUNDO ST', valor: this.vlrMoeda(this.item.tributacoes.fundo_st_total, \"BRL\"), blExibe: this.blExibeFundoSt },\n          { tributo: 'CIDE Combustíveis', valor: this.vlrMoeda(this.item.tributacoes.cide_total, \"BRL\"), blExibe: this.blExibeCide }\n        ];\n      },\n\n      arIdsAdicoes() {\n        return this.item.idsAdicoes.split(',').map(id => {\n          return Number(id);\n        });\n      },\n\n      arAdicoes() {\n        return this.resultado.adicoes.filter(ad => {\n          return this.arIdsAdicoes.indexOf(ad.id) !== -1;\n        });\n      },\n\n      blExibeIcmsSt() {   \n        let blExibeSt = false;\n        this.arAdicoes.forEach(ad => {\n          if (ad.tributacoes.icms.id_tipo_tributacao == 7) {\n            blExibeSt = true;\n          }\n        });\n        return blExibeSt && this.item.tributacoes.icms_st_total > 0;\n      },\n\n      blExibeFundo() {\n        let blExibeFundo = false;\n        this.arAdicoes.forEach(ad => {\n          if (ad.tributacoes.fundo_combate_pobreza.aliquota != null && ad.tributacoes.icms.id_tipo_tributacao != 7) {\n            blExibeFundo = true;\n          }\n        });\n        return blExibeFundo;\n      },\n\n      blExibeFundoSt() {\n        let blExibeFundoSt = false;\n        this.arAdicoes.forEach(ad => {\n          if (ad.tributacoes.fundo_combate_pobreza.aliquota != null && ad.tributacoes.icms.id_tipo_tributacao == 7) {\n            blExibeFundoSt = true;\n          }\n        });\n        return blExibeFundoSt && this.item.tributacoes.fundo_st_total > 0;\n      },\n\n      blExibeCide() {\n        let blExibeCide = false;\n        this.arAdicoes.forEach(ad => {\n          if (ad.tributacoes.cide_combustiveis.valor > 0) {\n            blExibeCide = true;\n          }\n        });\n        return blExibeCide;\n      },\n    },\n  }\n</script>"],"sourceRoot":"src/components/QuadrosResultado"}]}