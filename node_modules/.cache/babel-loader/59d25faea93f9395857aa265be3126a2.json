props=\"{\n          showFirstLastPage: false\n        }\"\n        dense\n      >\n        <template v-slot:item=\"{ item }\">\n          <tr>\n            <td>{{ item.id_gerado }}</td>\n            <td>{{ vlrMoeda(item.vmcv_real, \"BRL\") }}</td>\n            <td>{{ vlrMoeda(item.valor_total_mercadoria, \"BRL\") }}</td>\n            <td>{{ vlrMoeda(item.frete_real, \"BRL\") }}</td>\n            <td>{{ vlrMoeda(item.seguro_internacional, \"BRL\") }}</td>\n            <td v-if=\"tipoOperacao == 'Marítima'\">{{ vlrMoeda(item.capatazia, \"BRL\") }}</td>\n            <td>{{ vlrMoeda(item.valor_aduaneiro, \"BRL\") }}</td>\n            <td v-if=\"tipoOperacao == 'Marítima'\">{{ vlrMoeda(item.afrmm, \"BRL\") }}</td>\n            <td>{{ vlrMoeda(item.taxa_siscomex, \"BRL\") }}</td>\n            <td>{{ vlrMoeda(item.outras_despesas, \"BRL\") }}</td>\n          </tr>\n        </template>\n\n        <template slot=\"body.append\">\n          <tr>\n            <th class=\"text-caption font-weight-bold\" style=\"padding-left: 0\">Total</th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">\n              {{ vlrMoeda(totais_custos_operacao.vmcv, \"BRL\") }}\n            </th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">\n              {{ vlrMoeda(totais_custos_operacao.valor_total_mercadoria, \"BRL\") }}\n            </th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">\n              {{ vlrMoeda(totais_custos_operacao.frete_internacional, \"BRL\") }}\n            </th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">\n              {{ vlrMoeda(totais_custos_operacao.seguro_internacional, \"BRL\") }}\n            </th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\" v-if=\"tipoOperacao == 'Marítima'\" >\n              {{ vlrMoeda(totais_custos_operacao.capatazia, \"BRL\") }}\n            </th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">\n              {{ vlrMoeda(totais_custos_operacao.valor_aduaneiro, \"BRL\") }}\n            </th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\" v-if=\"tipoOperacao == 'Marítima'\">\n              {{ vlrMoeda(totais_custos_operacao.afrmm, \"BRL\") }}\n            </th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">\n              {{ vlrMoeda(totais_custos_operacao.siscomex, \"BRL\") }}\n            </th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">\n              {{ vlrMoeda(totais_custos_operacao.outras_despesas, \"BRL\") }}\n            </th>\n          </tr>\n        </template>\n      </v-data-table>\n    </v-card-text>\n  </v-card>\n</template>\n<script>\n  import formataMoedaMixin from \"@/mixins/formataMoedaMixin\";\n\n  export default {\n    name: \"QuadroRateioCustosItem\",\n\n    mixins: [formataMoedaMixin],\n\n    props: [\"tipoOperacao\"],\n\n    computed: {\n      headersCustos: {\n        get() {\n          const headerCustos = [];\n          headerCustos.push({ text: \"#\", value: \"id\", sortable: false });\n          headerCustos.push({ text: \"VMCV\", value: \"vmcv\", sortable: false });\n          headerCustos.push({\n            text: \"Valor da Mercadoria (VMLE)\",\n            value: \"valor_mercadoria\",\n            sortable: false,\n          });\n          headerCustos.push({\n            text: \"Frete Internacional\",\n            value: \"frete_internacional\",\n            sortable: false,\n          });\n          headerCustos.push({\n            text: \"Seguro Internacional\",\n            value: \"seguro_internacional\",\n            sortable: false,\n          });\n          if (this.tipoOperacao == \"Marítima\") {\n            headerCustos.push({\n              text: \"Capatazia/THC\",\n              value: \"capatazia\",\n              sortable: false,\n            });\n          }\n          headerCustos.push({\n            text: \"Valor Aduaneiro (VMLD)\",\n            value: \"valor_aduaneiro\",\n            sortable: false,\n          });\n          if (this.tipoOperacao == \"Marítima\") {\n            headerCustos.push({ text: \"AFRMM\", value: \"afrmm\", sortable: false });\n          }\n          headerCustos.push({\n            text: \"Taxa SISCOMEX\",\n            value: \"taxa_siscomex\",\n            sortable: false,\n          });\n          headerCustos.push({\n            text: \"Outras Despesas\",\n            value: \"outras_despesas\",\n            sortable: false,\n          });\n\n          return headerCustos;\n        },\n        set(h) {\n          return h;\n        },\n      },\n\n      custos_operacao: {\n        get() {\n          return this.$store.state.resultadosFinal.custos_operacao;\n        },\n        set(newValue) {\n          if (newValue != this.custos_operacao) {\n            this.$store.commit(\"resultadosFinal/setCustosOperacao\", newValue);\n          }\n        },\n      },\n      totais_custos_operacao: {\n        get() {\n          return this.$store.state.resultadosFinal.totais_custos_operacao;\n        },\n        set(newValue) {\n          if (newValue != this.totais_custos_operacao) {\n            this.$store.commit(\n              \"resultadosFinal/setTotaisCustosOperacao\",\n              newValue\n            );\n          }\n        },\n      },\n    },\n  };\n</script>"],"sourceRoot":"src/components/QuadrosResultado"}]}