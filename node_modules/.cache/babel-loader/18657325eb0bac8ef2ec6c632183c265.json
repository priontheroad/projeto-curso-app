mx-4 text-caption font-weight-bold formata-color-padrao\">\n      <v-col cols=\"4\">Tributos</v-col>\n      <v-col cols=\"4\" align=\"center\">Base de CÃ¡lculo</v-col>\n      <v-col cols=\"4\" align=\"end\">Total</v-col>\n    </v-row>\n\n    <v-divider class=\"my-2 ml-4 color-divider\"></v-divider>\n\n    <v-row justify=\"center\" no-gutters class=\"ml-8 mx-4 my-2\" v-for=\"(tributo, index) in tributos\" :key=\"'tributo' + index\">\n      <v-col cols=\"4\" class=\"text-caption formata-color-cinza font-weight-medium\">{{ tributo.tributo }}</v-col>\n      <v-col cols=\"4\" align=\"center\" class=\"text-caption formata-color-cinza font-weight-medium\">{{ tributo.base_calculo }}</v-col>\n      <v-col cols=\"4\" align=\"end\" class=\"text-caption font-weight-medium\">{{ tributo.total }}</v-col>\n    </v-row>\n\n    <v-divider class=\"mt-4 ml-4 color-divider\" v-if=\"blOutrosTributos\"></v-divider>\n\n    <v-row justify=\"center\" no-gutters class=\"ml-8 mx-4 my-2\" v-if=\"blOutrosTributos\">\n      <v-col class=\"text-caption font-weight-bold\">Outros Tributos</v-col>\n    </v-row>\n\n    <v-row justify=\"center\" no-gutters class=\"ml-8 mx-4 my-2\" v-for=\"(tributo, index) in outrosTributos\" :key=\"'outros' + index\">\n      <v-col cols=\"6\">\n        <span class=\"text-caption formata-color-cinza ml-4 font-weight-medium\">{{ tributo.tributo }}</span>\n      </v-col>\n      <v-col cols=\"6\" align=\"end\">\n        <span class=\"text-caption formata-color-padrao font-weight-medium\">\n          {{ vlrMoeda(dados_gerais.tributacoes.icms_st.total, \"BRL\") }}\n        </span>\n      </v-col>\n    </v-row>\n  </div>\n</template>\n<script>\n  import { mapGetters } from 'vuex';\n  import formataMoedaMixin from \"@/mixins/formataMoedaMixin\";\n\n  export default {\n    name: 'QuadroTributosDadosGerais',\n\n    mixins: [formataMoedaMixin],\n   \n    computed: {\n      ...mapGetters(\"resultadosFinal\", {\n        dados_gerais: \"getDadosGerais\",\n      }),\n\n      tributos() {\n        return [\n          {\n            tributo:      'II',\n            base_calculo: this.vlrMoeda(this.dados_gerais.tributacoes.ii.base_calculo, \"BRL\"),\n            total:        this.vlrMoeda(this.dados_gerais.tributacoes.ii.total, \"BRL\")\n          },\n          {\n            tributo:      'IPI',\n            base_calculo: this.vlrMoeda(this.dados_gerais.tributacoes.ipi.base_calculo, \"BRL\"),\n            total:        this.vlrMoeda(this.dados_gerais.tributacoes.ipi.total, \"BRL\")\n          },\n          {\n            tributo:      'PIS - ImportaÃ§Ã£o',\n            base_calculo: this.vlrMoeda(this.dados_gerais.tributacoes.pis.base_calculo, \"BRL\"),\n            total:        this.vlrMoeda(this.dados_gerais.tributacoes.pis.total, \"BRL\")\n          },\n          {\n            tributo:      'COFINS - ImportaÃ§Ã£o',\n            base_calculo: this.vlrMoeda(this.dados_gerais.tributacoes.cofins.base_calculo, \"BRL\"),\n            total:        this.vlrMoeda(this.dados_gerais.tributacoes.cofins.total, \"BRL\")\n          },\n          {\n            tributo:      'ICMS',\n            base_calculo: this.vlrMoeda(this.dados_gerais.tributacoes.icms.base_calculo, \"BRL\"),\n            total:        this.vlrMoeda(this.dados_gerais.tributacoes.icms.total, \"BRL\")\n          },\n        ];\n      },\n\n      outrosTributos() {\n        let outrosTributos = [];\n\n        if (this.dados_gerais.tributacoes.cide_combustiveis.total > 0) {\n          outrosTributos.push({\n            tributo: 'CIDE COMBUSTIVEIS',\n            total:   this.vlrMoeda(this.dados_gerais.tributacoes.cide_combustiveis.total, \"BRL\")\n          });\n        }\n\n        if (this.dados_gerais.tributacoes.icms_st.total > 0) {\n          outrosTributos.push({\n            tributo: 'ICMS-ST',\n            total:   this.vlrMoeda(this.dados_gerais.tributacoes.icms_st.total, \"BRL\")\n          });\n        }\n\n        if (this.dados_gerais.tributacoes.fundo.total > 0) {\n          outrosTributos.push({\n            tributo: 'Fundo de Combate Ã  Pobreza',\n            total:   this.vlrMoeda(this.dados_gerais.tributacoes.fundo.total, \"BRL\")\n          });\n        }\n\n        if (this.dados_gerais.tributacoes.fundo_st.total > 0) {\n          outrosTributos.push({\n            tributo: 'Fundo de Combate Ã  Pobreza ST',\n            total:   this.vlrMoeda(this.dados_gerais.tributacoes.fundo_st.total, \"BRL\")\n          });\n        }\n        \n       return outrosTributos;\n      },\n\n      blOutrosTributos() {\n        return this.outrosTributos.length > 0;\n      },\n    }\n  }\n</script>"],"sourceRoot":"src/components/QuadrosResultado"}]}