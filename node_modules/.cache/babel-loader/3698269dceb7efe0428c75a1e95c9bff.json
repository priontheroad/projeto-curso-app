utrasDespesas\") {\n        validacao = this.verificaFormularioCustosImportacao();\n        if (validacao == false) {\n          //back\n          this.alterarAba(\"aba-CustosImportacao\");\n        } else {\n          //next\n          this.alterarAba(\"aba-OutrasDespesas\");\n        }\n      }\n    },\n    alterarAba(aba) {\n      this.alterarAba = aba;\n    },\n    handleModalConsulta(val) {\n      if (val == true) {\n        this.abaAtual = 'aba-DadosGerais';\n      }\n    }\n  },\n  computed: {\n    ...mapGetters(\"modalConsulta\", {\n      handleModalConsulta: \"getModalConsulta\",\n    }),\n    abaAtual: {\n      get() {\n        return this.$store.state.aba.abaAtual;\n      },\n      set(newValue) {\n        if (newValue != this.abaAtual) {\n          this.$store.commit(\"aba/setAbaAtual\", newValue);\n        }\n      },\n    },\n  },\n  methods: {\n    verificaFormularioDadosGerais() {\n      if (\n        typeof this.$refs.abaDadosGerais !== 'undefined' &&\n        typeof this.$refs.abaDadosGerais[0] !== 'undefined'\n      ) {\n        return this.$refs.abaDadosGerais[0].verificaFormularioDadosGerais();\n      }\n      return false;\n    },\n    verificaFormularioCustosImportacao() {\n      if (\n        typeof this.$refs.abaCustosImportacao !== 'undefined' &&\n        typeof this.$refs.abaCustosImportacao[0] !== 'undefined'\n      ) {\n        return this.$refs.abaCustosImportacao[0].verificaFormularioCustosImportacao();\n      }\n      return false;\n    },\n    verificaFormularioOutrasDespesas() {\n      if (\n        typeof this.$refs.abaOutrasDespesas !== 'undefined' &&\n        typeof this.$refs.abaOutrasDespesas[0] !== 'undefined'\n      ) {\n        return this.$refs.abaOutrasDespesas[0].verificaFormularioOutrasDespesas();\n      }\n      return false;\n    },\n    verificaLinhaAberta() {\n      if (\n        typeof this.$refs.abaOutrasDespesas !== 'undefined' &&\n        typeof this.$refs.abaOutrasDespesas[0] !== 'undefined'\n      ) {\n        return this.$refs.abaOutrasDespesas[0].verificaLinhaAberta();\n      }\n      return false;\n    },\n    alterarAba(aba) {\n      setTimeout(() => {\n        this.abaAtual = aba;\n      }, 100);\n    },\n  },\n}"]}]}