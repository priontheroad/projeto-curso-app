ops=\"{ contentClass: 'valor-mercadoria' }\"\n          v-model=\"formConsulta.valor_mercadoria_moeda\"\n          class=\"rounded-r-0\"\n          placeholder=\"Moeda\"\n          outlined\n          dense\n          item-value=\"sigla_moeda\"\n          item-text=\"descricao\"\n          :items=\"tipo_moeda\"\n          :error-messages=\"mercadoriaMoedaErrors\"\n          :key=\"indexRenderizacao\"\n        />\n      </v-col>\n      <v-col cols=\"4\">\n        <CampoMonetario\n          v-model=\"formConsulta.valor_mercadoria_valor\"\n          :intDecimais=\"2\"\n          :appendIcon=\"' '\"\n          class=\"rounded-l-0\"\n          :errorMessages=\"mercadoriaValorErrors\"\n        />\n      </v-col>\n    </v-row>\n        \n    <v-row no-gutters justify=\"center\">\n      <v-col cols=\"2\">\n        <label class=\"text-caption font-weight-bold mr-1\">Frete Internacional</label>\n      </v-col>\n      <v-col cols=\"3\">\n        <v-select\n          :menu-props=\"{ contentClass: 'frete-internacional' }\"\n          v-model=\"formConsulta.frete_internacional_moeda\"\n          class=\"rounded-r-0\"\n          placeholder=\"Moeda\"\n          outlined\n          dense\n          item-value=\"sigla_moeda\"\n          item-text=\"descricao\"\n          :items=\"tipo_moeda\"\n          :key=\"indexRenderizacao\"\n        />\n      </v-col>\n      <v-col cols=\"4\">\n        <CampoMonetario\n          v-model=\"formConsulta.frete_internacional_valor\"\n          :intDecimais=\"2\"\n          :appendIcon=\"' '\"\n          class=\"rounded-l-0\"\n        />\n      </v-col>\n    </v-row>\n\n    <v-row no-gutters justify=\"center\">\n      <v-col cols=\"2\">\n        <label class=\"text-caption font-weight-bold mr-1\">Seguro Internacional</label>\n      </v-col>\n      <v-col cols=\"3\">\n        <v-select\n          :menu-props=\"{ contentClass: 'seguro-internacional' }\"\n          v-model=\"formConsulta.seguro_internacional_moeda\"\n          class=\"rounded-r-0\"\n          placeholder=\"Moeda\"\n          outlined\n          dense\n          :items=\"tipo_moeda\"\n          item-value=\"sigla_moeda\"\n          item-text=\"descricao\"\n          :key=\"indexRenderizacao\"\n        />\n      </v-col>\n      <v-col cols=\"4\">\n        <CampoMonetario\n          v-model=\"formConsulta.seguro_internacional_valor\"\n          :intDecimais=\"2\"\n          :appendIcon=\"' '\"\n          class=\"rounded-l-0\"\n        />\n      </v-col>\n    </v-row>\n\n    <template v-if=\"formConsulta.tipo_operacao == $AQUAVIARIO.ID\">\n      <v-row no-gutters justify=\"center\">\n        <v-col cols=\"9\">\n          <v-divider></v-divider>\n        </v-col>\n      </v-row>\n\n      <v-row justify=\"center\" no-gutters class=\"mt-2\">\n        <v-col cols=\"3\">\n          <label class=\"text-caption font-weight-bold\">Capatazia/THC            \n          <v-tooltip top>\n            <template v-slot:activator=\"{ on, attrs }\">\n              <v-icon small color=\"#FFC400\" dark v-bind=\"attrs\" v-on=\"on\">mdi-alert-outline</v-icon>\n            </template>\n            <p>A Capatazia ou THC (Tax Handling Container), é uma taxa cobrada pelos armadores, considerada um gasto relativo à descarga e manuseio da mercadoria importada.</p>\n          </v-tooltip>\n          </label>        \n          <CampoMonetario v-model=\"formConsulta.capatazia\" />\n        </v-col>\n        <v-col cols=\"3\" class=\"pl-1\">\n          <label class=\"text-caption font-weight-bold\">AFRMM\n            <v-tooltip top>\n            <template v-slot:activator=\"{ on, attrs }\">\n              <v-icon small color=\"#FFC400\" dark v-bind=\"attrs\" v-on=\"on\">mdi-alert-outline</v-icon>\n            </template>\n            <p>O AFRMM (Adicional ao Frete para a Renovação da Marinha Mercante) é calculado sobre o valor do frete da operação de importação, aplicado o percentual de 8% nas navegações de longo curso.</p>\n          </v-tooltip>\n          </label>\n          <CampoMonetario v-model=\"formConsulta.afrmm\" />\n        </v-col>\n        <v-col cols=\"3\"></v-col>\n      </v-row>\n      <v-row justify=\"center\" no-gutters>\n        <v-col cols=\"9\">\n          <v-checkbox \n            v-model=\"formConsulta.blSomaCapataziaVMLD\" \n            label=\"Incluir Capatazia/THC no Valor Aduaneiro (VMLD)\"\n          />\n        </v-col>\n      </v-row>\n    </template>\n  </div>\n</template>\n<script>\nimport { mapGetters } from \"vuex\";\nexport default {\n  validations: {\n    \"formConsulta.valor_mercadoria_moeda\": {\n      checked() {\n        let moeda_mercadoria = this.formConsulta.valor_mercadoria_moeda;\n        if (moeda_mercadoria === null || moeda_mercadoria === \"\") {\n          return false;\n        } else {\n          return true;\n        }\n      },\n    },\n    \"formConsulta.valor_mercadoria_valor\": {\n      checked() {\n        let valor_mercadoria = this.formConsulta.valor_mercadoria_valor;\n        if (\n          valor_mercadoria === null ||\n          valor_mercadoria === \"\" ||\n          valor_mercadoria <= 0\n        ) {\n          return false;\n        } else {\n          return true;\n        }\n      },\n    },\n  },\n  watch: {\n    handleModalConsulta(val) {\n      //abrindo criar formConsulta\n      if (val == true) {\n        // limpar inputs quando usuario esta criando uma nova declaracao\n        if (this.editandoModalConsulta == false) {\n          //limpar validacoes\n          this.$v.$reset();\n        }\n\n        //renderizar inputs novamente\n        this.indexRenderizacao++;\n      }\n    },\n  },\n  data() {\n    return {\n      indexRenderizacao: 0,\n    };\n  },\n  computed: {\n    ...mapGetters(\"informacoesPadrao\", {\n      tipo_moeda: \"getTipoMoeda\",\n    }),\n    ...mapGetters(\"formConsulta\", {\n      formConsulta: \"getState\",\n    }),\n    ...mapGetters(\"modalConsulta\", {\n      handleModalConsulta: \"getModalConsulta\",\n      editandoModalConsulta: \"getEditando\",\n    }),\n    mercadoriaMoedaErrors() {\n      const errors = [];\n      if (!this.$v[\"formConsulta.valor_mercadoria_moeda\"].$dirty) return errors;\n      !this.$v[\"formConsulta.valor_mercadoria_moeda\"].checked &&\n        errors.push(\"Preenchimento obrigatório.\");\n      return errors;\n    },\n    mercadoriaValorErrors() {\n      const errors = [];\n      if (!this.$v[\"formConsulta.valor_mercadoria_valor\"].$dirty) return errors;\n      !this.$v[\"formConsulta.valor_mercadoria_valor\"].checked &&\n        errors.push(\"Preenchimento obrigatório.\");\n      return errors;\n    },\n  },\n  methods: {\n    verificaFormularioCustosImportacao() {\n      this.$v.$touch();\n      return (!this.$v.$invalid);\n    },\n  },\n};\n</script>\n<style lang=\"scss\">\n.v-tooltip__content {\n  height: fit-content !important;\n}\n</style>"],"sourceRoot":"src/views/Formularios/CustosImportacao"}]}