  flex-row\n          justify-center\n          align-center\n          justify-space-between\n          historico__img\n        \"\n      >\n        <div class=\"d-flex align-center standard-title\">\n          Consultas\n          <v-btn\n            @click.stop=\"validarAberturaModalConsulta()\"\n            color=\"#2d5071\"\n            class=\"text-capitalize ml-6 elevation-0\"\n          >\n            <span class=\"botao-nova-simulacao\">Criar Novo</span>\n            <v-icon right color=\"white\">mdi-plus-circle-outline</v-icon>\n          </v-btn>\n        </div>\n        <div class=\"half d-flex \">\n        </div>\n      </div>\n      <div class=\"mt-2\">\n        <TabelaHistorico />\n      </div>\n    </div>\n  </div>\n</template>\n<script defer>\nimport { mapActions, mapGetters } from \"vuex\";\nimport Form from \"@/services/modules/form\";\n\nexport default {\n  name: \"Historico\",\n  components: {\n    TabelaHistorico: () => import('./TabelaHistorico/TabelaHistorico.vue'),\n  },\n  data() {\n    return {\n    };\n  },\n  computed: {\n    ...mapGetters(\"informacoesPadrao\", {\n      status_carregamento: \"getStatusCarregamento\"\n    }),\n    ...mapGetters(\"modal\", [\"getShowModalConsulta\"]),\n    ...mapGetters('informacoesPadrao', {\n      info: \"getState\"\n    }),\n    ...mapGetters(\"auth\", {\n      nome_usuario: \"getNomeUsuario\",\n      id_usuario: \"getIdUsuario\",\n    }),\n    modalTermo: {\n      get() {\n        return this.$store.state.termo.modalTermo;\n      },\n      set(newValue) {\n        if (newValue != this.modalTermo) {\n          this.$store.commit(\"termo/setModalTermo\", newValue);\n        }\n      },\n    },\n    snackbarTermo: {\n      get() {\n        return this.$store.state.termo.snackbarTermo;\n      },\n      set(newValue) {\n        if (newValue != this.snackbarTermo) {\n          this.$store.commit(\"termo/setsnackbarTermo\", newValue);\n        }\n      },\n    },\n    handleNovaSimulacao: {\n      get() {\n        return this.$store.state.modalNovaSimulacao.modalNovaSimulacao;\n      },\n      set(newValue) {\n        if (newValue != this.handleNovaSimulacao) {\n          this.$store.commit(\"modalNovaSimulacao/setModalNovaSimulacao\", newValue);\n        }\n      },\n    },\n  },\n  methods: {\n    ...mapActions(\"modal\", [\"toggleShowModalConsulta\"]),\n\n    validarAberturaModalConsulta(){\n      //verificar se informacoesPadroes carregaram\n      if(this.status_carregamento == false){\n        Object.bus.$emit(\"structure\", {\n          message: \"Erro ao carregar informações, entre em contato com a econet.\",\n          type: \"error\",\n          timeout: 10000,\n        });\n        this.handleNovaSimulacao = false;\n        return false;\n      }\n\n      if(localStorage.getItem(\"aceite_termo\")){\n        //zerar Dados Gerais\n        let form = new Form();\n        form.carregarNovaConsulta();\n        this.handleNovaSimulacao = true;\n        this.snackbarTermo = false;\n      }else{\n        this.handleNovaSimulacao = false;\n        this.modalTermo = true;\n        this.snackbarTermo = true;\n      }\n    },\n  },\n};\n</script>\n<style lang=\"scss\">\n@import url(\"./Historico.scss\");\n</style>"]}]}