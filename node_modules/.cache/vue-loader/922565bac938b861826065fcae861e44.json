ia -->\n          <v-col cols=\"4\">\n            <span class=\"text-caption font-weight-medium\"\n              >Valor da Mercadoria</span\n            >\n          </v-col>\n          <template v-if=\"dados_gerais.valor_mercadoria.moeda == 'BRL'\">\n            <v-col cols=\"3\" offset=\"5\" align=\"end\">\n              <span\n                class=\"\n                  text-caption\n                  formata-color-padrao\n                  mr-8\n                  font-weight-medium\n                \"\n              >\n                {{ vlrMoeda(dados_gerais.valor_mercadoria.valor_real, \"BRL\") }}\n              </span>\n            </v-col>\n          </template>\n          <template v-else>\n            <v-col cols=\"3\" align=\"center\">\n              <span class=\"text-caption formata-color-cinza font-weight-medium\">\n                {{\n                  vlrCodigoMoeda(\n                    dados_gerais.valor_mercadoria.valor_moeda,\n                    dados_gerais.valor_mercadoria.moeda\n                  )\n                }}\n              </span>\n            </v-col>\n            <v-col cols=\"2\" class=\"d-flex justify-center\">\n              <v-icon small color=\"#97A3AE\">mdi-arrow-right</v-icon>\n            </v-col>\n            <v-col cols=\"3\" align=\"end\">\n              <span\n                class=\"\n                  text-caption\n                  formata-color-padrao\n                  mr-8\n                  font-weight-medium\n                \"\n              >\n                {{ vlrMoeda(dados_gerais.valor_mercadoria.valor_real, \"BRL\") }}\n              </span>\n            </v-col>\n          </template>\n        </v-row>\n\n        <!-- Frete Internacional -->\n        <v-row justify=\"center\" no-gutters class=\"my-2\">\n          <v-col cols=\"4\">\n            <span class=\"text-caption formata-color-padrao font-weight-medium\">Frete Internacional</span>\n          </v-col>\n          <template v-if=\"dados_gerais.frete_internacional.moeda == 'BRL'\">\n            <v-col cols=\"3\" offset=\"5\" align=\"end\">\n              <span class=\"text-caption formata-color-padrao mr-8 font-weight-medium\">\n                {{ vlrMoeda(dados_gerais.frete_internacional.valor_real, \"BRL\") }}\n              </span>\n            </v-col>\n          </template>\n          <template v-else>\n            <v-col cols=\"3\">\n              <span class=\"text-caption formata-color-cinza font-weight-medium d-flex justify-center\">\n                {{ vlrCodigoMoeda(dados_gerais.frete_internacional.valor_moeda, dados_gerais.frete_internacional.moeda) }}\n              </span>\n            </v-col>\n            <v-col cols=\"2\" align=\"center\">\n              <v-icon small color=\"#97A3AE\">mdi-arrow-right</v-icon>\n            </v-col>\n            <v-col cols=\"3\" align=\"end\">\n              <span class=\"text-caption mr-8 font-weight-medium\">\n                {{ vlrMoeda(dados_gerais.frete_internacional.valor_real, \"BRL\") }}\n              </span>\n            </v-col>\n          </template>\n        </v-row>\n\n        <!-- Seguro Internacional -->\n        <v-row justify=\"center\" no-gutters class=\"my-2\">\n          <v-col cols=\"4\">\n            <span class=\"text-caption font-weight-medium\">Seguro Internacional</span>\n          </v-col>\n          <template v-if=\"dados_gerais.seguro_internacional.moeda == 'BRL'\">\n            <v-col cols=\"3\" offset=\"5\" align=\"end\">\n              <span class=\"text-caption formata-color-padrao mr-8 font-weight-medium\">\n                {{ vlrMoeda(dados_gerais.seguro_internacional.valor_real, \"BRL\") }}\n              </span>\n            </v-col>\n          </template>\n          <template v-else>\n            <v-col cols=\"3\">\n              <span class=\"text-caption formata-color-cinza font-weight-medium d-flex justify-center\">\n                {{ vlrCodigoMoeda(dados_gerais.seguro_internacional.valor_moeda, dados_gerais.seguro_internacional.moeda) }}\n              </span>\n            </v-col>\n            <v-col cols=\"2\" align=\"center\">\n              <v-icon small color=\"#97A3AE\">mdi-arrow-right</v-icon>\n            </v-col>\n            <v-col cols=\"3\" align=\"end\">\n              <span class=\"text-caption formata-color-padrao mr-8 font-weight-medium\">\n                {{ vlrMoeda(dados_gerais.seguro_internacional.valor_real, \"BRL\") }}\n              </span>\n            </v-col>\n          </template>\n        </v-row>\n\n        <v-divider class=\"my-6 mr-4 color-divider\"></v-divider>\n\n        <!-- Subtotal CIF -->\n        <div class=\"d-flex row justify-space-between mr-2 align-center\">\n          <div class=\"ml-4\">\n            <span class=\"text-caption font-weight-bold formata-color-padrao\"\n              >Subtotal CIF</span\n            >\n          </div>\n          <div class=\"mr-6\">\n            <span class=\"text-caption font-weight-bold formata-color-azul\">\n              {{ vlrMoeda(dados_gerais.subtotal_cif, \"BRL\") }}\n            </span>\n          </div>\n        </div>\n\n        <v-divider class=\"my-6 mr-4 color-divider\"></v-divider>\n\n        <!-- Capatazia -->\n        <div\n          v-if=\"tipoOperacao == 'Marítima'\"\n          class=\"d-flex row justify-space-between mr-2 align-center\"\n        >\n          <div class=\"ml-4\">\n            <span class=\"text-caption formata-color-padrao font-weight-medium\"\n              >Capatazia/THC</span\n            >\n          </div>\n          <div class=\"mr-6\">\n            <span class=\"text-caption formata-color-padrao font-weight-medium\">\n              {{ vlrMoeda(dados_gerais.capatazia.valor_real, \"BRL\") }}\n            </span>\n          </div>\n        </div>\n\n        <v-divider\n          v-if=\"tipoOperacao == 'Marítima'\"\n          class=\"my-6 mr-4 color-divider\"\n        ></v-divider>\n\n        <!-- Valor Aduaneiro -->\n        <div class=\"d-flex row justify-space-between mr-2 align-center\">\n          <div class=\"ml-4\">\n            <span class=\"text-caption font-weight-bold\"\n              >Valor Aduaneiro (VMLD)</span\n            >\n          </div>\n          <div class=\"mr-6\">\n            <span class=\"text-caption font-weight-bold formata-color-azul\">\n              {{ vlrMoeda(dados_gerais.valor_aduaneiro, \"BRL\") }}\n            </span>\n          </div>\n        </div>\n\n        <v-divider class=\"my-6 mr-4 color-divider\"></v-divider>\n\n        <!-- Outros -->\n        <div class=\"d-flex row justify-space-between mr-2 mb-2 align-center\">\n          <div class=\"ml-4\">\n            <span class=\"text-caption font-weight-bold formata-color-padrao\"\n              >Outros</span\n            >\n          </div>\n        </div>\n\n        <!--  AFRMM - Adicional ao Frete para Renovação da Marinha Mercante -->\n        <div\n          v-if=\"tipoOperacao == 'Marítima'\"\n          class=\"d-flex row justify-space-between mr-2 align-center my-4\"\n        >\n          <div class=\"ml-4\">\n            <span\n              class=\"text-caption ml-2 formata-color-padrao font-weight-medium\"\n            >\n              AFRMM - Adicional ao Frete para Renovação da Marinha Mercante\n            </span>\n          </div>\n          <div class=\"mr-6\">\n            <span class=\"text-caption formata-color-padrao font-weight-medium\">\n              {{ vlrMoeda(dados_gerais.afrmm.valor_real, \"BRL\") }}\n            </span>\n          </div>\n        </div>\n\n        <!-- Outras Despesas -->\n        <div class=\"d-flex row justify-space-between mr-2 align-center my-4\">\n          <div class=\"ml-4\">\n            <span class=\"text-caption ml-2 font-weight-medium\"\n              >Outras Despesas</span\n            >\n          </div>\n          <div class=\"mr-6\">\n            <span class=\"text-caption formata-color-padrao font-weight-medium\">\n              {{ vlrMoeda(dados_gerais.outras_despesas.valor_moeda, \"BRL\") }}\n            </span>\n          </div>\n        </div>\n\n        <!-- Taxa Siscomex -->\n        <div class=\"d-flex row justify-space-between mr-2 align-center my-4\">\n          <div class=\"ml-4\">\n            <span\n              class=\"text-caption ml-2 formata-color-padrao font-weight-medium\"\n              >Taxa Siscomex</span\n            >\n          </div>\n          <div></div>\n          <div class=\"mr-6\">\n            <span class=\"text-caption formata-color-padrao font-weight-medium\">\n              {{ vlrMoeda(dados_gerais.taxa_siscomex, \"BRL\") }}\n            </span>\n          </div>\n        </div>\n      </v-col>\n\n      <v-divider vertical class=\"color-divider\"></v-divider>\n\n      <!-- Coluna Direita -->\n      <v-col cols=\"6\">\n        <QuadroTributosDadosGerais />\n      </v-col>\n    </v-row>\n  </div>\n</template>\n<script>\nimport { mapGetters } from \"vuex\";\nimport formataMoedaMixin from \"@/mixins/formataMoedaMixin\";\nimport QuadroTributosDadosGerais from \"@/components/QuadrosResultado/QuadroTributosDadosGerais.vue\";\nexport default {\n  nome: \"DadosGeraisResultado\",\n  components: { QuadroTributosDadosGerais },\n  props: [\"tipoOperacao\"],\n  data() {\n    return {};\n  },\n  mixins: [formataMoedaMixin],\n  computed: {\n    ...mapGetters(\"resultadosFinal\", {\n      dados_gerais: \"getDadosGerais\",\n      estado: \"getEstado\",\n    }),\n  },\n  filters: {\n    truncate: function (text, length, suffix) {\n      if (\n        typeof text !== \"undefined\" &&\n        text !== null &&\n        text !== \"\" &&\n        text.length > length\n      ) {\n        return text.substring(0, length) + suffix;\n      } else {\n        return text;\n      }\n    },\n  },\n};\n</script>\n"]}]}