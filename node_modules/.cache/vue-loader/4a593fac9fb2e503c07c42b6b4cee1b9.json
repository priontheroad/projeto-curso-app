ers=\"headerTaxas\"\n        :items=\"tributacoes_taxas\"\n        :items-per-page=5\n        :footer-props=\"{\n          showFirstLastPage: false\n        }\"\n        dense\n      >\n        <template v-slot:item=\"{ item }\"> \n          <tr>\n            <td>{{ item.id_gerado }}</td>\n            <td>{{ vlrMoeda(item.ii, \"BRL\") }}</td>\n            <td>{{ vlrMoeda(item.ipi, \"BRL\") }}</td>\n            <td>{{ vlrMoeda(item.pis, \"BRL\") }}</td>\n            <td>{{ vlrMoeda(item.cofins, \"BRL\") }}</td>\n            <td>{{ vlrMoeda(item.base_calculo_icms, \"BRL\") }}</td>\n            <td>{{ vlrMoeda(item.icms, \"BRL\") }}</td>\n            <td v-if=\"adicoes_informacoes.tributacoes.icms.id_tipo_tributacao == 7\">{{ vlrMoeda(item.icms_st, \"BRL\") }}</td>\n            <td v-if=\"adicoes_informacoes.tributacoes.icms.id_tipo_tributacao == 10\">{{ vlrMoeda(item.icms_reducao, \"BRL\") }}</td>\n            <td v-if=\"adicoes_informacoes.tributacoes.icms.id_tipo_tributacao == 8\">{{ vlrMoeda(item.icms_diferimento, \"BRL\") }}</td>\n            <td>{{ vlrMoeda(item.icms_desconto, \"BRL\") }}</td>\n            <td>{{ vlrMoeda(item.icms_fundo, \"BRL\") }}</td>\n            <td v-if=\"adicoes_informacoes.tributacoes.icms.id_tipo_tributacao == 7\">{{ vlrMoeda(item.icms_fundo_st, \"BRL\") }}</td>\n            <td>{{ vlrMoeda(item.cide_combustiveis, \"BRL\") }}</td>\n          </tr>\n        </template>\n\n        <template slot=\"body.append\">\n          <tr>\n            <th class=\"text-caption font-weight-bold\" style=\"padding-left: 0\">Total</th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">{{ vlrMoeda(totais_tributacoes_taxas.ii, \"BRL\") }}</th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">{{ vlrMoeda(totais_tributacoes_taxas.ipi, \"BRL\") }}</th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">{{ vlrMoeda(totais_tributacoes_taxas.pis, \"BRL\") }}</th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">{{ vlrMoeda(totais_tributacoes_taxas.cofins, \"BRL\") }}</th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">{{ vlrMoeda(totais_tributacoes_taxas.base_calculo_icms, \"BRL\") }}</th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">{{ vlrMoeda(totais_tributacoes_taxas.icms, \"BRL\") }}</th>\n            <th v-if=\"adicoes_informacoes.tributacoes.icms.id_tipo_tributacao == 7\" class=\"text-caption font-weight-bold formata-color-azul alinha-total\">\n              {{ vlrMoeda(totais_tributacoes_taxas.icms_st, \"BRL\") }}\n            </th>\n            <th v-if=\"adicoes_informacoes.tributacoes.icms.id_tipo_tributacao == 10\" class=\"text-caption font-weight-bold formata-color-azul alinha-total\">\n              {{ vlrMoeda(totais_tributacoes_taxas.icms_reducao_bc, \"BRL\") }}\n            </th>\n            <th v-if=\"adicoes_informacoes.tributacoes.icms.id_tipo_tributacao == 8\" class=\"text-caption font-weight-bold formata-color-azul alinha-total\">\n              {{ vlrMoeda(totais_tributacoes_taxas.icms_diferimento, \"BRL\") }}\n            </th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">{{ vlrMoeda(totais_tributacoes_taxas.icms_desconto, \"BRL\") }}</th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">{{ vlrMoeda(totais_tributacoes_taxas.fundo, \"BRL\") }}</th>\n            <th v-if=\"adicoes_informacoes.tributacoes.icms.id_tipo_tributacao == 7\" class=\"text-caption font-weight-bold formata-color-azul alinha-total\">\n              {{ vlrMoeda(totais_tributacoes_taxas.fundo_st, \"BRL\") }}\n            </th>\n            <th class=\"text-caption font-weight-bold formata-color-azul alinha-total\">{{ vlrMoeda(totais_tributacoes_taxas.cide_combustiveis, \"BRL\") }}</th>\n          </tr>\n          \n        </template>\n      </v-data-table>\n    </v-card-text>\n  </v-card>\n</template>\n<script>\n  import formataMoedaMixin from \"@/mixins/formataMoedaMixin\";\n\n  export default {\n    name: 'QuadroRateioTributacoesItem',\n\n    mixins: [formataMoedaMixin],\n\n    props: [ \"tipoOperacao\" ],\n\n    computed: {\n      headerTaxas: {\n        get() {\n          const headerTaxas = [];\n          headerTaxas.push({ text: \"#\", value: \"id\", sortable: false });\n          headerTaxas.push({ text: \"II\", value: \"ii\", sortable: false });\n          headerTaxas.push({ text: \"IPI\", value: \"ipi\", sortable: false });\n          headerTaxas.push({ text: \"PIS\", value: \"pis\", sortable: false });\n          headerTaxas.push({ text: \"COFINS\", value: \"cofins\", sortable: false });\n          headerTaxas.push({ text: \"Base Cálculo ICMS\", value: \"base_calculo_icms\", sortable: false });\n          headerTaxas.push({ text: \"ICMS\", value: \"icms\", sortable: false });\n          if (this.adicoes_informacoes.tributacoes.icms.id_tipo_tributacao == 7) {\n            headerTaxas.push({ text: \"ICMS-ST\", value: \"icms_st\", sortable: false });\n          }\n          if (this.adicoes_informacoes.tributacoes.icms.id_tipo_tributacao == 10) {\n            headerTaxas.push({ text: \"ICMS - Redução da BC\", value: \"icms_reducao\", sortable: false });\n          }\n          if (this.adicoes_informacoes.tributacoes.icms.id_tipo_tributacao == 8) {\n            headerTaxas.push({ text: \"ICMS - Diferimento\", value: \"icms_diferimento\", sortable: false });\n          }\n          headerTaxas.push({ text: \"Desconto (ICMS)\", value: \"icms_desconto\", sortable: false });\n          headerTaxas.push({ text: \"Fundo de comb. à Probreza\", value: \"icms_fundo\", sortable: false });\n          if (this.adicoes_informacoes.tributacoes.icms.id_tipo_tributacao == 7) {\n            headerTaxas.push({ text: \"Fundo de comb. à Probreza - ST\", value: \"icms_fundo_st\", sortable: false });\n          }\n          headerTaxas.push({ text: \"CIDE\", value: \"cide_combustiveis\", sortable: false });\n\n          return headerTaxas;\n        },\n        set(v) {\n          return v;\n        }\n      },\n\n      adicoes_informacoes: {\n        get() {\n          return this.$store.state.resultadosFinal.adicoes_informacoes;\n        },\n        set(newValue) {\n          if (newValue != this.adicoes_informacoes) {\n            this.$store.commit(\"resultadosFinal/setAdicoesInformacoes\", newValue);\n          }\n        },\n      },\n\n      tributacoes_taxas: {\n        get() {\n          return this.$store.state.resultadosFinal.tributacoes_taxas;\n        },\n        set(newValue) {\n          if (newValue != this.tributacoes_taxas) {\n            this.$store.commit(\"resultadosFinal/setTributacoesTaxas\", newValue);\n          }\n        },\n      },\n\n      totais_tributacoes_taxas: {\n        get() {\n          return this.$store.state.resultadosFinal.totais_tributacoes_taxas;\n        },\n        set(newValue) {\n          if (newValue != this.totais_tributacoes_taxas) {\n            this.$store.commit(\"resultadosFinal/setTotaisTributacoesTaxas\", newValue);\n          }\n        },\n      },\n    },\n  }\n</script>"]}]}