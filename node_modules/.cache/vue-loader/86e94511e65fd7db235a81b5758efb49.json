;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAu3BA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Origem.vue","sourceRoot":"src/views/Formularios/Origem","sourcesContent":["<template>\n  <v-row justify=\"center\">\n    <v-col cols=\"8\">\n      <div>\n        <v-row justify=\"center\">\n          <v-col cols=\"12\">\n            <div class=\"mt-1\">\n              <span class=\"d-flex justify-center informativos-tela\">Adicione os dados da origem da adição.</span>\n              <span class=\"informativos-tela d-flex justify-center\"><strong>Campos com <b>*</b> são obrigatórios.</strong></span>\n            </div>\n          </v-col>\n        </v-row>\n        <v-row no-gutters justify=\"center\">\n          <v-col cols=\"2\">\n            <label for=\"\" class=\"text-caption font-weight-bold\">N° Adição</label>\n            <v-text-field\n              placeholder=\"N°\"\n              class=\"mr-2\"\n              outlined\n              dense\n              :disabled=\"dinamicaCampoOrdem()\"\n              v-model=\"ordem\"\n              :error-messages=\"ordemErrors\"\n            />\n          </v-col>\n          <v-col cols=\"6\">\n            <label class=\"text-caption\">Incoterm <b>*</b></label>\n            <v-select\n              v-model=\"id_incoterm\"\n              :items=\"incoterm_items_aux\"\n              item-value=\"id\"\n              item-text=\"descricao\"\n              placeholder=\"Selecione\"\n              dense\n              outlined\n              class=\"mr-3\"\n              :error-messages=\"incotermErrors\"\n            />\n          </v-col>\n\n          <v-col cols=\"4\">\n            <label class=\"text-caption ml-2\">Moeda <b>*</b></label>\n            <v-select\n              :menu-props=\"{ contentClass: 'moeda' }\"\n              v-model=\"moeda\"\n              :items=\"tipo_moeda\"\n              item-value=\"sigla_moeda\"\n              item-text=\"descricao\"\n              placeholder=\"Selecione\"\n              class=\"ml-2\"\n              dense\n              outlined\n              :error-messages=\"moedaErrors\"\n            />\n          </v-col>\n        </v-row>\n        <v-row\n          v-if=\"id_incoterm == $DAP.ID || id_incoterm == $DPU.ID\"\n          no-gutters\n          justify=\"center\"\n        >\n          <v-col>\n            <v-checkbox\n              v-model=\"adicao.seguro_vmcv\"\n              label=\"Valor do seguro está incluso no VMCV (Valor da Mercadoria na Condição de Venda).\"\n            />\n          </v-col>\n        </v-row>\n        <v-row no-gutters justify=\"center\" class=\"mt-6\">\n          <v-col cols=\"8\">\n            <label class=\"text-caption\">Exportador <b>*</b></label>\n            <v-combobox\n              :menu-props=\"{ contentClass: 'exportador' }\"\n              dense\n              v-model=\"modelExportador\"\n              :items=\"exportador.itemsExportador\"\n              :loading=\"exportador.isLoadingExportador\"\n              :search-input.sync=\"searchExportador\"\n              item-text=\"nome\"\n              outlined\n              single-line\n              placeholder=\"Digite ...\"\n              append-icon=\"\"\n              @blur=\"AdicionaExportador\"\n              @keydown.tab=\"AdicionaExportador\"\n              @keydown.enter=\"AdicionaExportador\"\n              @keyup.enter=\"AdicionaExportador\"\n              @keyup.tab=\"AdicionaExportador\"\n              :error-messages=\"modelExportadorErrors\"\n              return-object\n              ref=\"selectExportador\"\n              maxlength=\"255\"\n            >\n              <template v-slot:no-data>\n                <template v-if=\"searchExportador !== null && searchExportador !== ''\">\n                  <v-list-item>\n                    <v-list-item-title>\n                      <i>Item não cadastrado...</i>\n                    </v-list-item-title>\n                  </v-list-item>\n                </template>\n\n                <template v-else>\n                  <v-list-item>\n                    <v-list-item-title>\n                      <i>Pesquise o Exportador.</i>\n                    </v-list-item-title>\n                  </v-list-item>\n                </template>\n              </template>\n\n              <template v-slot:item=\"data\">\n                <div style=\"display: flex\">\n                  <div>\n                    {{ data.item.nome }}\n                  </div>\n                  <div class=\"ml-5\">\n                    <v-icon\n                      color=\"red\"\n                      small\n                      right\n                      v-if=\"\n                        !data.item.adicao_count &&\n                        !data.item.adicao_fabricante_count\n                      \"\n                      @click.stop=\"showDeleteDialog(data.item.id)\"\n                      >mdi-delete-outline</v-icon\n                    >\n                  </div>\n                </div>\n              </template>\n            </v-combobox>\n          </v-col>\n          <v-col cols=\"4\">\n            <label class=\"text-caption ml-2\">País de Aquisição <b>*</b></label>\n            <v-autocomplete\n              v-model=\"modelPais\"\n              :items=\"paises\"\n              item-text=\"nomePais\"\n              item-value=\"id\"\n              placeholder=\"Selecione\"\n              class=\"ml-2\"\n              dense\n              outlined\n              @input=\"alteraPaisExportador\"\n              :error-messages=\"modelPaisExportadorErrors\"\n            />\n          </v-col>\n        </v-row>\n\n        <v-row no-gutters justify=\"center\">\n          <v-col cols=\"12\">\n            <v-checkbox\n              label=\"Exportador também é Fabricante/Produtor.\"\n              v-model=\"exportador_fabricante\"\n            />\n            <v-row no-gutters v-if=\"exportador_fabricante == false\">\n              <v-col cols=\"8\">\n                <label class=\"text-caption\">Fabricante/Produtor <b>*</b></label>\n                <v-combobox\n                  :menu-props=\"{ contentClass: 'importador' }\"\n                  dense\n                  v-model=\"modelFabricante\"\n                  :items=\"exportador.itemsExportador\"\n                  :loading=\"fabricante.isLoadingFabricante\"\n                  :search-input.sync=\"searchFabricante\"\n                  item-text=\"nome\"\n                  outlined\n                  single-line\n                  placeholder=\"Digite ...\"\n                  append-icon=\"\"\n                  @keydown.tab=\"AdicionaFabricante\"\n                  @keydown.enter=\"AdicionaFabricante\"\n                  @keyup.enter=\"AdicionaFabricante\"\n                  @keyup.tab=\"AdicionaFabricante\"\n                  @blur=\"AdicionaFabricante\"\n                  :error-messages=\"modelFabricanteErrors\"\n                  return-object\n                  ref=\"selectFabricante\"\n                  maxlength=\"255\"\n                >\n                  <template v-slot:no-data>\n                    <template\n                      v-if=\"\n                        searchFabricante !== null &&\n                        searchFabricante !== '' &&\n                        typeof searchFabricante !== 'undefined'\n                      \"\n                    >\n                      <v-list-item>\n                        <v-list-item-title>\n                          <i>Item não cadastrado...</i>\n                        </v-list-item-title>\n                      </v-list-item>\n                    </template>\n\n                    <template v-else>\n                      <v-list-item>\n                        <v-list-item-title>\n                          <i>Pesquise o Fabricante.</i>\n                        </v-list-item-title>\n                      </v-list-item>\n                    </template>\n                  </template>\n\n                  <template v-slot:item=\"data\">\n                    <div style=\"display: flex\">\n                      <div>\n                        {{ data.item.nome }}\n                      </div>\n                      <div class=\"ml-5\">\n                        <v-icon\n                          color=\"red\"\n                          small\n                          right\n                          v-if=\"\n                            !data.item.adicao_count &&\n                            !data.item.adicao_fabricante_count\n                          \"\n                          @click=\"showDeleteDialog(data.item.id)\"\n                          >mdi-delete-outline</v-icon\n                        >\n                      </div>\n                    </div>\n                  </template>\n                  <!-- <template v-slot:append-item>\n                    <template\n                      v-if=\"\n                        searchFabricante !== null && searchFabricante !== ''\n                      \"\n                    >\n                      <v-list-item>\n                        <v-list-item-title>\n                          <v-list-item-content>\n                            <v-btn\n                              text\n                              color=\"primary\"\n                              style=\"font-weight: 400\"\n                              depressed\n                              @click=\"AdicionaFabricante($event)\"\n                            >\n                              Cadastrar Fabricante\n                              <v-icon small right>mdi-plus</v-icon>\n                            </v-btn>\n                          </v-list-item-content>\n                        </v-list-item-title>\n                      </v-list-item>\n                    </template>\n                  </template> -->\n                </v-combobox>\n              </v-col>\n              <v-col cols=\"4\">\n                <label class=\"text-caption ml-2\">País de Origem <b>*</b></label>\n                <v-autocomplete\n                  v-model=\"modelPaisFabricante\"\n                  :items=\"paises\"\n                  placeholder=\"Selecione\"\n                  item-text=\"nomePais\"\n                  item-value=\"id\"\n                  dense\n                  outlined\n                  class=\"ml-2\"\n                  @input=\"alteraPaisFabricante\"\n                  :error-messages=\"modelPaisFabricanteErrors\"\n                />\n              </v-col>\n            </v-row>\n          </v-col>\n        </v-row>\n      </div>\n    </v-col>\n    <!-- snackbar exclusao exportador/fabricante -->\n    <SnackBarDelete\n      :snackbar=\"dialogDelete\"\n      :message=\"'Você tem certeza que deseja excluir este Exportador/Fabricante?'\"\n      @cancelar_snackbar=\"dialogDelete = false\"\n      @confirmar_snackbar=\"deletarExportadorFabricante()\"\n    />\n  </v-row>\n</template>\n<script>\nimport { mapGetters } from \"vuex\";\nimport { required } from \"vuelidate/lib/validators\";\nimport SnackBarDelete from \"@/components/SnackBar/SnackBarDelete.vue\";\n\nexport default {\n  name: \"Origem\",\n  components: {\n    SnackBarDelete,\n  },\n  props: { adicaoEscolhida: Object },\n  validations: {\n    ordem: {\n      checked() {\n        if (this.ordem > this.adicoes.dados.length && !this.adicao.id == \"\") {\n          return false;\n        } else {\n          return true;\n        }\n      },\n    },\n    modelExportador: {\n      checked() {\n        if (\n          this.modelExportador == null ||\n          this.modelExportador == \"\" ||\n          (typeof this.modelExportador == \"object\" &&\n            this.modelExportador.nome == \"\")\n        ) {\n          return false;\n        } else {\n          return true;\n        }\n      },\n      verificaQuantidadeCaracteres() {\n        if (\n          this.modelExportador == null ||\n          this.modelExportador == \"\" ||\n          (typeof this.modelExportador == \"object\" &&\n            this.modelExportador.nome == \"\")\n        ) {\n          return true;\n        } else {\n          if (\n            typeof this.modelExportador == \"object\" &&\n            this.modelExportador.nome.length > 255\n          ) {\n            return false;\n          } else {\n            return true;\n          }\n        }\n      },\n    },\n    modelPais: {\n      checked() {\n        if (\n          this.modelPais == null ||\n          this.modelPais == \"\" ||\n          isNaN(this.modelPais) == true\n        ) {\n          return false;\n        } else {\n          return true;\n        }\n      },\n    },\n    modelFabricante: {\n      checked() {\n        if (this.exportador_fabricante == true) {\n          return true;\n        } else {\n          if (\n            this.modelFabricante == null ||\n            this.modelFabricante == \"\" ||\n            (typeof this.modelFabricante == \"object\" &&\n              this.modelFabricante.nome == \"\")\n          ) {\n            return false;\n          } else {\n            return true;\n          }\n        }\n      },\n      verificaQuantidadeCaracteres() {\n        if (\n          this.modelFabricante == null ||\n          this.modelFabricante == \"\" ||\n          (typeof this.modelFabricante == \"object\" &&\n            this.modelFabricante.nome == \"\")\n        ) {\n          return true;\n        } else {\n          if (\n            typeof this.modelFabricante == \"object\" &&\n            this.modelFabricante.nome.length > 255\n          ) {\n            return false;\n          } else {\n            return true;\n          }\n        }\n      },\n    },\n    modelPaisFabricante: {\n      checked() {\n        if (this.exportador_fabricante == true) {\n          return true;\n        } else {\n          if (\n            this.modelPaisFabricante == null ||\n            this.modelPaisFabricante == \"\" ||\n            isNaN(this.modelPaisFabricante) == true\n          ) {\n            return false;\n          } else {\n            return true;\n          }\n        }\n      },\n    },\n    id_incoterm: {\n      required,\n    },\n    moeda: {\n      required,\n    },\n  },\n  mounted() {\n    if (this.handleModalAdicao == true) {\n      this.carregandoInformacoesPreenchidas();\n      this.$store.dispatch(\"exportador/listarExportadores\");\n      if (!this.adicao.id) {\n        this.searchExportador = this.exportadorSelecionado;\n        this.AdicionaExportador();\n        this.searchFabricante = this.fabricanteSelecionado;\n        this.AdicionaFabricante();\n        this.modelPais = this.paisExportadorSelecionado;\n        this.modelPaisFabricante = this.paisFabricanteSelecionado;\n      }\n    }\n  },\n  data() {\n    return {\n      incoterm_items_aux: [],\n      dialogDelete: false,\n      itemToDelete: \"\",\n      modelExportador: null,\n      searchExportador: null,\n      timeoutExportador: \"\",\n      searchFabricante: null,\n      modelFabricante: null,\n      timeoutFabricante: \"\",\n    };\n  },\n  computed: {\n    ...mapGetters(\"consulta\", {\n      consulta: \"getState\",\n    }),\n    ...mapGetters(\"exportador\", {\n      exportador: \"getState\",\n    }),\n\n    ...mapGetters(\"fabricante\", {\n      fabricante: \"getState\",\n    }),\n    ...mapGetters(\"informacoesPadrao\", {\n      lista_estados: \"getListaEstados\",\n      tipo_operacao: \"getTipoOperacao\",\n      incoterm_items: \"getIncoterm\",\n      paises: \"getPaises\",\n      tipo_moeda: \"getTipoMoeda\",\n    }),\n    ...mapGetters(\"adicao\", {\n      adicao: \"getState\",\n    }),\n    ...mapGetters(\"adicoes\", {\n      adicoes: \"getState\",\n    }),\n    ...mapGetters(\"modalAdicoes\", {\n      handleModalAdicao: \"getModalAdicoes\",\n    }),\n    ...mapGetters('abaAdicoes', {\n      exportadorSelecionado: 'getExportadorSelecionado',\n      fabricanteSelecionado: 'getEabricanteSelecionado',\n      paisFabricanteSelecionado: 'getPaisFabricanteSelecionado',\n      paisExportadorSelecionado: 'getPaisExportadorSelecionado',\n    }),\n    ordem: {\n      get() {\n        if (this.$store.state.adicao.id > 0) {\n          return this.$store.state.adicao.ordem;\n        } else {\n          let total = this.adicoes.dados.length + 1;\n          this.$store.commit(\"adicao/setOrdem\", total);\n\n          return total;\n        }\n      },\n      set(newValue) {\n        if (newValue != this.ordem) {\n          this.$store.commit(\"adicao/setOrdem\", newValue);\n        }\n      },\n    },\n    tipo_operacao: {\n      get() {\n        return this.$store.state.consulta.tipo_operacao;\n      },\n      set(newValue) {\n        if (newValue != this.tipo_operacao) {\n          this.$store.commit(\"consulta/setTipoOperacao\", newValue);\n        }\n      },\n    },\n    itemsExportador: {\n      get() {\n        return this.$store.state.exportador.itemsExportador;\n      },\n      set(newValue) {\n        if (newValue != this.itemsExportador) {\n          this.$store.commit(\"exportador/setItemsExportador\", newValue);\n        }\n      },\n    },\n    itemsFabricante: {\n      get() {\n        return this.$store.state.fabricante.itemsFabricante;\n      },\n      set(newValue) {\n        if (newValue != this.itemsFabricante) {\n          this.$store.commit(\"fabricante/setItemsFabricante\", newValue);\n        }\n      },\n    },\n    exportador_fabricante: {\n      get() {\n        return this.$store.state.adicao.exportador_fabricante;\n      },\n      set(newValue) {\n        if (newValue != this.exportador_fabricante) {\n          this.$store.commit(\"adicao/setExportadorFabricante\", newValue);\n        }\n      },\n    },\n    id_incoterm: {\n      get() {\n        return this.$store.state.adicao.id_incoterm;\n      },\n      set(newValue) {\n        if (newValue != this.id_incoterm) {\n          this.$store.commit(\"adicao/setIdIncoterm\", newValue);\n        }\n      },\n    },\n    moeda: {\n      get() {\n        return this.$store.state.adicao.moeda;\n      },\n      set(newValue) {\n        if (newValue != this.moeda) {\n          this.$store.commit(\"adicao/setMoeda\", newValue);\n        }\n      },\n    },\n    modelPais: {\n      get() {\n        return this.$store.state.adicao.id_pais_exportador;\n      },\n      set(newValue) {\n        this.$store.commit(\"adicao/setPaisExportador\", newValue);\n      },\n    },\n    modelPaisFabricante: {\n      get() {\n        return this.$store.state.adicao.id_pais_fabricante;\n      },\n      set(newValue) {\n        this.$store.commit(\"adicao/setPaisFabricante\", newValue);\n      },\n    },\n    ordemErrors() {\n      const errors = [];\n      if (!this.$v.ordem.$dirty) return errors;\n      !this.$v.ordem.checked && errors.push(\"Valor de ordem inexistente\");\n      return errors;\n    },\n\n    modelExportadorErrors() {\n      const errors = [];\n      if (!this.$v.modelExportador.$dirty) return errors;\n      !this.$v.modelExportador.checked &&\n        errors.push(\"Exportador é obrigatório.\");\n      !this.$v.modelExportador.verificaQuantidadeCaracteres &&\n        errors.push(\"Quantidade máxima de preenchimento atingida.\");\n      return errors;\n    },\n    modelPaisExportadorErrors() {\n      const errors = [];\n      if (!this.$v.modelPais.$dirty) return errors;\n      !this.$v.modelPais.checked && errors.push(\"País é obrigatório.\");\n      return errors;\n    },\n    modelFabricanteErrors() {\n      const errors = [];\n      if (!this.$v.modelFabricante.$dirty) return errors;\n      !this.$v.modelFabricante.checked &&\n        errors.push(\"Fabricante é obrigatório.\");\n      !this.$v.modelFabricante.verificaQuantidadeCaracteres &&\n        errors.push(\"Quantidade máxima de preenchimento atingida.\");\n      return errors;\n    },\n    modelPaisFabricanteErrors() {\n      const errors = [];\n      if (!this.$v.modelPaisFabricante.$dirty) return errors;\n      !this.$v.modelPaisFabricante.checked &&\n        errors.push(\"País é obrigatório.\");\n      return errors;\n    },\n    incotermErrors() {\n      const errors = [];\n      if (!this.$v.id_incoterm.$dirty) return errors;\n      !this.$v.id_incoterm.required && errors.push(\"Incoterm é obrigatório.\");\n      return errors;\n    },\n    moedaErrors() {\n      const errors = [];\n      if (!this.$v.moeda.$dirty) return errors;\n      !this.$v.moeda.required && errors.push(\"Moeda é obrigatório.\");\n      return errors;\n    },\n  },\n  watch: {\n    handleModalAdicao(val) {\n      if (val == true) {\n        this.carregandoInformacoesPreenchidas();\n        this.$store.dispatch(\"exportador/listarExportadores\");\n        this.limpaErros();\n      }\n    },\n    \"consulta.tipo_operacao\"() {\n      this.filtroIncoterm();\n    },\n    modelExportador() {\n      //seleciona um item da lista\n      // popula exportador e pais de aquisicao no modulo de adicao\n      if (\n        this.modelExportador !== null &&\n        this.modelExportador !== \"\" &&\n        typeof this.modelExportador == \"object\"\n      ) {\n        let objeto_selecionado = {};\n        this.itemsExportador.forEach((element) => {\n          if (element.nome == this.modelExportador.nome) {\n            objeto_selecionado = element;\n          }\n        }, objeto_selecionado);\n\n        //popula exportador no modulo da adicao\n        this.adicao.exportador = {\n          nome: objeto_selecionado.nome,\n        };\n      }\n    },\n    modelFabricante() {\n      if (\n        this.modelFabricante !== null &&\n        this.modelFabricante !== \"\" &&\n        typeof this.modelFabricante == \"object\"\n      ) {\n        let objeto_selecionado = {};\n        this.fabricante.itemsFabricante.forEach((element) => {\n          if (element.nome == this.modelFabricante.nome) {\n            objeto_selecionado = element;\n          }\n        }, objeto_selecionado);\n\n        this.adicao.fabricante = {\n          nome: objeto_selecionado.nome,\n        };\n      }\n    },\n    searchFabricante(val) {\n      if (\n        typeof val !== \"undefined\" &&\n        typeof val == \"string\" &&\n        val.trim() !== \"\"\n      ) {\n        if (this.fabricante.itemsFabricante.length > 0) {\n          //verificar compatibilidade\n          let pesquisa = val;\n          let flag = false;\n\n          this.fabricante.itemsFabricante.forEach(\n            (element) => {\n              if (element.nome.toLowerCase() == pesquisa.toLowerCase()) {\n                flag = true;\n              }\n            },\n            pesquisa,\n            flag\n          );\n\n          if (flag == true) {\n            return false;\n          }\n        }\n\n        clearTimeout(this.timeoutFabricante);\n        this.timeoutFabricante = null;\n\n        this.timeoutFabricante = setTimeout(() => {\n          this.fabricante.isLoadingFabricante = true;\n          let dados = {\n            nome: this.searchFabricante,\n            // pais: \"\",\n            // codigo_pais: \"\",\n          };\n\n          this.$store.dispatch(\"fabricante/pesquisaFabricante\", dados);\n        }, 1000);\n      } else {\n        this.modelFabricante = {\n          nome: \"\",\n        };\n      }\n    },\n    searchExportador(val) {\n      if (\n        typeof val !== \"undefined\" &&\n        typeof val == \"string\" &&\n        val.trim() !== \"\"\n      ) {\n        if (this.exportador.itemsExportador.length > 0) {\n          //verificar compatibilidade\n          let pesquisa = val;\n          let flag = false;\n\n          this.exportador.itemsExportador.forEach(\n            (element) => {\n              if (element.nome.toLowerCase() == pesquisa.toLowerCase()) {\n                flag = true;