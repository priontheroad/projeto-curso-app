stify=\"center\" no-gutters class=\"mt-4\">\n      <v-col cols=\"4\">\n        <span class=\"text-caption font-weight-bold\">Incoterm</span>\n        <p class=\"text-caption font-weight-regular\">{{ adicoes_informacoes.incoterm.descricao }}</p>\n      </v-col>\n      <v-col cols=\"4\">\n        <span class=\"text-caption font-weight-bold\">Quant.</span>\n        <p class=\"text-caption font-weight-regular\">\n          {{ vlrDecimal(adicoes_informacoes.total_unidade_mercadoria) }} \n          {{ adicoes_informacoes.unidade_medida.sigla_unidade }}\n        </p>\n      </v-col>\n      <v-col cols=\"4\">\n        <span class=\"text-caption font-weight-bold\">Peso Líq. da Adição</span>\n        <p class=\"text-caption font-weight-regular\">{{ vlrDecimal(adicoes_informacoes.peso_liquido_total) }} kg</p>\n      </v-col>\n    </v-row>\n    \n    <v-divider></v-divider>\n\n    <v-row justify=\"center\" no-gutters class=\"mt-4\">\n      <v-col cols=\"6\">\n        <div class=\"text-caption font-weight-bold\">Exportador</div>\n        <div class=\"text-caption font-weight-regular upper\">\n          {{ adicoes_informacoes.exportador !== null ? adicoes_informacoes.exportador.nome : \"\" }}\n        </div>\n        <div class=\"text-caption font-weight-regular upper\">\n          {{ adicoes_informacoes.pais_exportador }}\n        </div>\n      </v-col>\n      <v-col cols=\"6\">\n        <div class=\"text-caption font-weight-bold\">Fabricante/Produtor</div>\n        <div class=\"text-caption font-weight-regular upper\">\n          {{ adicoes_informacoes.fabricante !== null ? adicoes_informacoes.fabricante.nome : adicoes_informacoes.exportador.nome }}\n        </div>\n        <div class=\"text-caption font-weight-regular upper\">\n          {{ adicoes_informacoes.pais_fabricante }}\n        </div>\n      </v-col>\n    </v-row>\n  </div>\n</template>\n<script>\n  import formataMoedaMixin from \"@/mixins/formataMoedaMixin\";\n\n  export default {\n    name: 'QuadroAdicaoDetalhes',\n\n    mixins: [formataMoedaMixin],\n    \n    computed: {\n      adicoes_informacoes: {\n        get() {\n          return this.$store.state.resultadosFinal.adicoes_informacoes;\n        },\n        set(newValue) {\n          if (newValue != this.adicoes_informacoes) {\n            this.$store.commit(\"resultadosFinal/setAdicoesInformacoes\", newValue);\n          }\n        },\n      },\n    }\n  }\n</script>"]}]}