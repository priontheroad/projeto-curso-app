<template>\n  <div>\n    <v-row justify=\"center\" class=\"mx-4 text-caption font-weight-bold formata-color-padrao\">\n      <v-col cols=\"3\">Tributos</v-col>\n      <v-col cols=\"2\">Tributação</v-col>\n      <v-col cols=\"2\">Alíquota</v-col>\n      <v-col cols=\"3\">Base de Cálculo</v-col>\n      <v-col cols=\"2\" align=\"end\">Total</v-col>\n    </v-row>\n\n    <v-divider class=\"my-2 ml-4 color-divider\"></v-divider>\n\n    <div v-for=\"(tributo, index) in tributos\" :key=\"index\">\n      <LinhaTributo \n        :tributo=\"tributo\" \n        v-if=\"tributo.blExibe\" \n      />\n    </div>\n\n    <v-divider class=\"mt-4 ml-4 color-divider\"></v-divider>\n\n    <v-row justify=\"center\" no-gutters class=\"ml-8 mx-4 my-2\">\n      <v-col cols=\"6\"><span class=\"text-caption font-weight-bold\">Total de Tributos</span></v-col>\n      <v-col cols=\"6\" align=\"end\">\n        <span class=\"text-caption font-weight-bold formata-color-azul\">\n          {{ vlrMoeda(adicoes_informacoes.total_tributos, \"BRL\") }}\n        </span>\n      </v-col>\n    </v-row>\n  </div>\n</template>\n<script>\n  import formataMoedaMixin from \"@/mixins/formataMoedaMixin\";\n  import LinhaTributo from \"@/components/QuadrosResultado/LinhaTributo.vue\";\n\n  export default {\n    name: 'QuadroTributosAdicao',\n\n    mixins: [formataMoedaMixin],\n\n    components: { LinhaTributo },\n\n    computed: {\n      adicoes_informacoes: {\n        get() {\n          return this.$store.state.resultadosFinal.adicoes_informacoes;\n        },\n        set(newValue) {\n          if (newValue != this.adicoes_informacoes) {\n            this.$store.commit(\"resultadosFinal/setAdicoesInformacoes\", newValue);\n          }\n        },\n      },\n\n      tributos() {\n        const tributos = [\n          {\n            tributo:      'II',\n            tributacao:   this.adicoes_informacoes.resultado_tributacoes.II.tributacao,\n            aliquota:     this.vlrDecimal(this.adicoes_informacoes.resultado_tributacoes.II.aliquota) + ' %',\n            base_calculo: this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.II.base_calculo, 'BRL'),\n            classeTotal:  '', //[1, 3, 5, 9, 12].indexOf(this.adicoes_informacoes.resultado_tributacoes.II.id_tributacao) !== -1 ? 'sublinhar' : '',\n            total:        this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.II.total, 'BRL'),\n            blExibe:      true\n          },\n          {\n            tributo:      'IPI',\n            tributacao:   this.adicoes_informacoes.resultado_tributacoes.IPI.tributacao,\n            aliquota:     this.vlrDecimal(this.adicoes_informacoes.resultado_tributacoes.IPI.aliquota) + ' %',\n            base_calculo: this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.IPI.base_calculo, 'BRL'),\n            classeTotal:  '', //[1, 3, 5, 9, 12].indexOf(this.adicoes_informacoes.resultado_tributacoes.IPI.id_tipo_tributacao) !== -1 ? 'sublinhar' : '',\n            total:        this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.IPI.total, 'BRL'),\n            blExibe:      true\n          },\n          {\n            tributo:      'PIS',\n            tributacao:   this.adicoes_informacoes.tributacoes.pis.tipo_tributacao.descricao,\n            aliquota:     this.vlrDecimal(this.adicoes_informacoes.resultado_tributacoes.PIS.aliquota) + ' %',\n            base_calculo: this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.PIS.base_calculo, 'BRL'),\n            classeTotal:  '', //[1, 3, 5, 9, 12].indexOf(this.adicoes_informacoes.resultado_tributacoes.PIS.id_tributacao) !== -1 ? 'sublinhar' : '',\n            total:        this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.PIS.total, 'BRL'),\n            blExibe:      true\n          },\n          {\n            tributo:      'COFINS',\n            tributacao:   this.adicoes_informacoes.tributacoes.cofins.tipo_tributacao.descricao,\n            aliquota:     this.vlrDecimal(this.adicoes_informacoes.resultado_tributacoes.COFINS.aliquota) + ' %',\n            base_calculo: this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.COFINS.base_calculo, 'BRL'),\n            classeTotal:  '', //[1, 3, 5, 9, 12].indexOf(this.adicoes_informacoes.resultado_tributacoes.PIS.id_tributacao) !== -1 ? 'sublinhar' : '',\n            total:        this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.COFINS.total, 'BRL'),\n            blExibe:      true\n          },\n          {\n            tributo:      'ICMS',\n            tributacao:   [8, 10, 11].indexOf(this.adicoes_informacoes.tributacoes.icms.tipo_tributacao.id) == -1 ? this.adicoes_informacoes.tributacoes.icms.tipo_tributacao.descricao : 'Integral',\n            aliquota:     this.vlrDecimal(this.adicoes_informacoes.resultado_tributacoes.ICMS.aliquota) + ' %',\n            base_calculo: [1, 3, 5, 9, 12].indexOf(this.adicoes_informacoes.tributacoes.icms.id_tipo_tributacao) == -1 ? this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.ICMS.base_calculo, 'BRL') : '--',\n            classeTotal:  '', //[1, 3, 5, 9, 12].indexOf(this.adicoes_informacoes.resultado_tributacoes.ICMS.id_tributacao) !== -1 ? 'sublinhar' : '',\n            total:        [1, 3, 5, 9, 12].indexOf(this.adicoes_informacoes.tributacoes.icms.id_tipo_tributacao) == -1 ? this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.ICMS.total, 'BRL') : '--',\n            blExibe:      true\n          },\n          {\n            tributo:      'ICMS-ST',\n            tributacao:   this.adicoes_informacoes.tributacoes.icms.tipo_tributacao.descricao,\n            aliquota:     this.vlrDecimal(this.adicoes_informacoes.resultado_tributacoes.ICMS_ST.aliquota) + ' %',\n            base_calculo: this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.ICMS_ST.base_calculo, \"BRL\"),\n            classeTotal:  '', //[1, 3, 5, 9, 12].indexOf(this.adicoes_informacoes.resultado_tributacoes.ICMS.id_tributacao) !== -1 ? 'sublinhar' : '',\n            total:        this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.ICMS_ST.total, \"BRL\"),\n            blExibe:      this.adicoes_informacoes.tributacoes.icms.tipo_tributacao.id == 7\n          },\n          {\n            tributo:      'ICMS - Redução da BC',\n            tributacao:   this.adicoes_informacoes.tributacoes.icms.tipo_tributacao.descricao,\n            aliquota:     this.vlrDecimal(this.adicoes_informacoes.resultado_tributacoes.REDUCAO_BC.aliquota) + ' %',\n            base_calculo: this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.REDUCAO_BC.base_calculo, \"BRL\"),\n            classeTotal:  '',\n            total:        this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.REDUCAO_BC.total, \"BRL\"),\n            blExibe:      this.adicoes_informacoes.tributacoes.icms.tipo_tributacao.id == 10\n          },\n          {\n            tributo:      'ICMS - Diferimento',\n            tributacao:   this.adicoes_informacoes.tributacoes.icms.tipo_tributacao.descricao,\n            aliquota:     this.vlrDecimal(this.adicoes_informacoes.resultado_tributacoes.DIFERIMENTO.aliquota) + ' %',\n            base_calculo: this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.DIFERIMENTO.base_calculo, \"BRL\"),\n            classeTotal:  '',\n            total:        this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.DIFERIMENTO.total, \"BRL\"),\n            blExibe:      this.adicoes_informacoes.tributacoes.icms.tipo_tributacao.id == 8\n          },\n          {\n            tributo:      'ICMS - Isenção Parcial',\n            tributacao:   this.adicoes_informacoes.tributacoes.icms.tipo_tributacao.descricao,\n            aliquota:     this.vlrDecimal(this.adicoes_informacoes.resultado_tributacoes.ISENCAO_PARCIAL.aliquota) + ' %',\n            base_calculo: this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.ISENCAO_PARCIAL.base_calculo, \"BRL\"),\n            classeTotal:  '',\n            total:        this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.ISENCAO_PARCIAL.total, \"BRL\"),\n            blExibe:      this.adicoes_informacoes.tributacoes.icms.tipo_tributacao.id == 11\n          },\n          {\n            tributo:      'ICMS TOTAL',\n            tributacao:   '--',\n            aliquota:     '--',\n            base_calculo: '--',\n            classeTotal:  '',\n            total:        this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.ICMS_TOTAL.total, \"BRL\"),\n            blExibe:      this.adicoes_informacoes.tributacoes.icms.tipo_tributacao.id != 2\n          },\n          {\n            tributo:      'Fundo de comb. à Probreza',\n            tributacao:   '--',\n            aliquota:     this.vlrDecimal(this.adicoes_informacoes.tributacoes.fundo_combate_pobreza.aliquota) + ' %',\n            base_calculo: '--',\n            classeTotal:  '',\n            total:        this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.FUNDO.total, \"BRL\"),\n            blExibe:      this.adicoes_informacoes.tributacoes.fundo_combate_pobreza.aliquota != null\n          },\n          {\n            tributo:      'Fundo de comb. à Probreza ST',\n            tributacao:   '--',\n            aliquota:     this.vlrDecimal(this.adicoes_informacoes.tributacoes.fundo_combate_pobreza.aliquota) + ' %',\n            base_calculo: '--',\n            classeTotal:  '',\n            total:        this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.FUNDO_ST.total, \"BRL\"),\n            blExibe:      this.adicoes_informacoes.tributacoes.fundo_combate_pobreza.aliquota != null && this.adicoes_informacoes.tributacoes.icms.tipo_tributacao.id == 7\n          },\n          {\n            tributo:      'CIDE Combustíveis',\n            tributacao:   '--',\n            aliquota:     '--',\n            base_calculo: '--',\n            classeTotal:  '',\n            total:        this.vlrMoeda(this.adicoes_informacoes.resultado_tributacoes.CIDE.total, \"BRL\"),\n            blExibe:      this.adicoes_informacoes.resultado_tributacoes.CIDE.total != ''\n          }\n        ];\n\n        return tributos;\n      },\n    }\n  }\n</script>"]}]}