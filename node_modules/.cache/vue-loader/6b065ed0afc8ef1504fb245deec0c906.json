AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"DadosGerais.vue","sourceRoot":"src/views/Formularios/DadosGerais","sourcesContent":["<template>\n  <!-- v-if para resetar preenchimento dos componentes abaixo-->\n  <v-row justify=\"center\">\n    <v-col cols=\"12\">\n      <div class=\"mt-3\">\n        <span class=\"d-flex justify-center informativos-tela\">\n          Informe os dados relativos a Declaração de Importação.\n        </span>\n        <span class=\"informativos-tela d-flex justify-center\">\n          <strong>\n            Campos com\n            <b style=\"color: #007cb3\">*</b>\n            são obrigatórios.</strong\n          >\n        </span>\n      </div>\n\n      <v-row no-gutters justify=\"center\" class=\"mt-2\">\n        <v-col cols=\"10\">\n          <v-text-field\n            v-model=\"formConsulta.descricao\"\n            class=\"centered-input\"\n            prepend-inner-icon=\"mdi-pencil-outline\"\n            placeholder=\"Adicionar Descrição\"\n            height=\"2.5rem\"\n            :key=\"indexRenderizacao\"\n            maxlength=\"255\"\n            :error-messages=\"descricaoErrors\"\n          ></v-text-field>\n        </v-col>\n      </v-row>\n      <v-row v-if=\"handleModalConsulta == true\" no-gutters justify=\"center\">\n        <v-col cols=\"4\">\n          <label class=\"text-caption font-weight-bold\"\n            >Número da DI/DUIMP</label\n          >\n          <v-text-field\n            v-mask=\"getMask()\"\n            class=\"code mr-4\"\n            dense\n            outlined\n            placeholder=\"Ex: 00/0000000-0\"\n            v-model=\"formConsulta.numero_declaracao\"\n            @keypress=\"uppercase\"\n            :key=\"indexRenderizacao\"\n          ></v-text-field>\n        </v-col>\n        <v-col cols=\"4\">\n          <label class=\"text-caption font-weight-bold\"\n            >Data da DI <b>*</b>\n          </label>\n\n          <CampoData\n            :value=\"data_di\"\n            :v=\"$v.data_di\"\n            v-model=\"data_di\"\n            :error_message=\"dataErrors\"\n            :check=\"true\"\n            :required=\"false\"\n          />\n        </v-col>\n        <v-col cols=\"2\">\n          <div class=\"ml-4\">\n            <label class=\"text-caption font-weight-bold\">UF <b>*</b></label>\n            <v-tooltip top>\n              <template v-slot:activator=\"{ on, attrs }\">\n                <v-icon small color=\"#FFC400\" right v-bind=\"attrs\" v-on=\"on\">\n                  mdi-alert-outline\n                </v-icon>\n              </template>\n              <span>Campo referente a UF do importador.</span>\n            </v-tooltip>\n            <v-select\n              placeholder=\"UF\"\n              dense\n              v-model=\"formConsulta.codigo_estado\"\n              :items=\"lista_estados\"\n              item-value=\"codigo_estado\"\n              item-text=\"sigla\"\n              outlined\n              :key=\"indexRenderizacao\"\n              :error-messages=\"codigoEstadoErrors\"\n            ></v-select>\n          </div>\n        </v-col>\n      </v-row>\n      <v-row justify=\"center\">\n        <v-col cols=\"2\">\n          <label class=\"text-caption font-weight-bold\">Adições <b>*</b></label>\n          <v-text-field\n            v-model=\"formConsulta.mercadoria_quantidade_total_adicoes\"\n            :maxlength=\"3\"\n            :rules=\"maxAdicao\"\n            type=\"number\"\n            min=\"1\"\n            @keypress=\"validateNumber\"\n            placeholder=\"Ex: 10\"\n            dense\n            outlined\n            :key=\"indexRenderizacao\"\n            :error-messages=\"qtdAdicoesErrors\"\n            v-mask=\"'##########'\"\n          ></v-text-field>\n        </v-col>\n        <v-col cols=\"4\">\n          <label class=\"text-caption font-weight-bold\">Peso Líquido <b>*</b></label>\n          <CampoMonetario \n            :intDecimais=\"5\" \n            v-model=\"formConsulta.mercadoria_peso_liquido_total\" \n            :appendIcon=\"'Kg'\"\n            :errorMessages=\"pesoLiquidoErrors\"\n          />\n        </v-col>\n        <v-col cols=\"4\">\n          <label class=\"text-caption font-weight-bold\">\n            Modal da Operação <b>*</b>\n          </label>\n          <v-select\n            :menu-props=\"{ contentClass: 'incoterm' }\"\n            class=\"modal_operacao\"\n            v-model=\"formConsulta.tipo_operacao\"\n            :items=\"lista_tipo_operacao\"\n            placeholder=\"Selecione\"\n            item-value=\"id\"\n            item-text=\"descricao\"\n            dense\n            outlined\n            :key=\"indexRenderizacao\"\n            :error-messages=\"tipoOperacaoErrors\"\n          ></v-select>\n        </v-col>\n      </v-row>\n    </v-col>\n  </v-row>\n</template>\n<script>\nimport { mapGetters } from \"vuex\";\nimport CampoData from \"../../../components/ArquivosDiversos/CampoData/CampoData.vue\";\n\nexport default {\n  name: \"DadosGerais\",\n  components: {\n    CampoData,\n  },\n  validations: {\n    \"formConsulta.descricao\": {\n      checked() {\n        let descricao = this.formConsulta.descricao;\n        if (descricao == null || descricao == \"\") {\n          return true;\n        } else {\n          //validar quantidade preenchida\n          if (descricao.length > 255) {\n            return false;\n          } else {\n            return true;\n          }\n        }\n      },\n    },\n    data_di: {\n      checked() {\n        let data = this.data_di;\n        if (data == null || data == \"\" || data.toString().length !== 10) {\n          return false;\n        } else {\n          //TODO:: futuramente fazer validacao, data colocada não pode ser maior ou igual que data atual\n          return true;\n        }\n      },\n    },\n    \"formConsulta.codigo_estado\": {\n      checked() {\n        let codigo_estado = this.formConsulta.codigo_estado;\n        if (codigo_estado == null || codigo_estado == \"\") {\n          return false;\n        } else {\n          return true;\n        }\n      },\n    },\n    \"formConsulta.mercadoria_quantidade_total_adicoes\": {\n      checked() {\n        let qtdAdicoes = this.formConsulta.mercadoria_quantidade_total_adicoes;\n        if (qtdAdicoes == null || qtdAdicoes == \"\") {\n          return false;\n        } else {\n          return true;\n        }\n      },\n      validaNumeros() {\n        let qtdAdicoes = this.formConsulta.mercadoria_quantidade_total_adicoes;\n        if (qtdAdicoes > 0) {\n          return true;\n        } else {\n          return false;\n        }\n      },\n    },\n    \"formConsulta.mercadoria_peso_liquido_total\": {\n      checked() {\n        let pesoAdicao = this.formConsulta.mercadoria_peso_liquido_total;\n        if (pesoAdicao == null || pesoAdicao == \"\") {\n          return false;\n        } else {\n          return true;\n        }\n      },\n    },\n    \"formConsulta.tipo_operacao\": {\n      checked() {\n        let tipo_operacao = this.formConsulta.tipo_operacao;\n        if (tipo_operacao == null || tipo_operacao == \"\") {\n          return false;\n        } else {\n          return true;\n        }\n      },\n    },\n  },\n  data() {\n    return {\n      maxAdicao: [\n        (v) => !!v || \"Qtd adições é obrigatório.\",\n        (v) => v && v >= 0,\n        (v) => (v && v <= 999) || \"Máximo: 999 adições\",\n      ],\n\n      indexRenderizacao: 0,\n      currency_config: {\n        decimal: \",\",\n        thousands: \".\",\n        decimalLength: 5,\n\n        masked: false,\n        allowBlank: false,\n      },\n    };\n  },\n  created() {},\n  mounted() {},\n  watch: {\n    handleModalConsulta(val) {\n      //abrindo criar formConsulta\n      if (val == true) {\n        // limpar inputs quando usuario esta criando uma nova declaracao\n        if (this.editandoModalConsulta == false) {\n          //limpar campos\n          this.formConsulta.descricao = \"\";\n          this.formConsulta.numero_declaracao = \"\";\n          this.formConsulta.data = \"\";\n          this.formConsulta.codigo_estado = \"\";\n          this.formConsulta.mercadoria_quantidade_total_adicoes = \"\";\n          this.formConsulta.mercadoria_peso_liquido_total = \"\";\n          this.formConsulta.tipo_operacao = \"\";\n\n          //limpar validacoes\n          this.$v.$reset();\n        }\n\n        //renderizar inputs novamente\n        this.indexRenderizacao++;\n      }\n    },\n\n    data_di(val) {\n      if (this.consulta.id == '') {\n        this.$store.commit('formConsulta/setBlSomaCapataziaVMLD', val <= '2022-06-08' ? 1 : 0);\n      }\n    },\n  },\n  computed: {\n    descricaoErrors() {\n      const errors = [];\n      if (!this.$v[\"formConsulta.descricao\"].$dirty) return errors;\n      !this.$v[\"formConsulta.descricao\"].checked &&\n        errors.push(\"Limite de preenchimento atingido.\");\n      return errors;\n    },\n    dataErrors() {\n      const errors = [];\n      if (!this.$v.data_di.$dirty) return errors;\n      !this.$v.data_di.checked && errors.push(\"Data é obrigatório.\");\n      return errors;\n    },\n    codigoEstadoErrors() {\n      const errors = [];\n      if (!this.$v[\"formConsulta.codigo_estado\"].$dirty) return errors;\n      !this.$v[\"formConsulta.codigo_estado\"].checked &&\n        errors.push(\"Estado é obrigatório.\");\n      return errors;\n    },\n    qtdAdicoesErrors() {\n      const errors = [];\n      if (!this.$v[\"formConsulta.mercadoria_quantidade_total_adicoes\"].$dirty)\n        return errors;\n      !this.$v[\"formConsulta.mercadoria_quantidade_total_adicoes\"].checked &&\n        errors.push(\"Qtd adições é obrigatório.\");\n      !this.$v[\"formConsulta.mercadoria_quantidade_total_adicoes\"]\n        .validaNumeros && errors.push(\"Qtd inválida.\");\n      return errors;\n    },\n    pesoLiquidoErrors() {\n      const errors = [];\n      if (!this.$v[\"formConsulta.mercadoria_peso_liquido_total\"].$dirty)\n        return errors;\n      !this.$v[\"formConsulta.mercadoria_peso_liquido_total\"].checked &&\n        errors.push(\"Peso líquido é obrigatório.\");\n      return errors;\n    },\n    tipoOperacaoErrors() {\n      const errors = [];\n      if (!this.$v[\"formConsulta.tipo_operacao\"].$dirty) return errors;\n      !this.$v[\"formConsulta.tipo_operacao\"].checked &&\n        errors.push(\"Modal operação é obrigatório.\");\n      return errors;\n    },\n\n    ...mapGetters(\"formConsulta\", {\n      formConsulta: \"getState\",\n    }),\n    ...mapGetters(\"consulta\", {\n      consulta: \"getState\",\n    }),\n    ...mapGetters(\"modalConsulta\", {\n      handleModalConsulta: \"getModalConsulta\",\n      editandoModalConsulta: \"getEditando\",\n    }),\n    ...mapGetters(\"informacoesPadrao\", {\n      lista_estados: \"getEstados\",\n      lista_tipo_operacao: \"getTipoOperacao\",\n      lista_incoterm: \"getIncoterm\",\n    }),\n    data_di: {\n      get() {\n        return this.$store.state.formConsulta.data;\n      },\n      set(newValue) {\n        if (newValue != this.data_di) {\n          this.$store.commit(\"formConsulta/setData\", newValue);\n        }\n      },\n    },\n    tipo_operacao: {\n      get() {\n        return this.$store.state.formConsulta.tipo_operacao;\n      },\n      set(newValue) {\n        if (newValue != this.tipo_operacao) {\n          this.$store.commit(\"formConsulta/setTipoOperacao\", newValue);\n        }\n      },\n    },\n    codigo_estado: {\n      get() {\n        return this.$store.state.formConsulta.codigo_estado;\n      },\n      set(newValue) {\n        if (newValue != this.codigo_estado) {\n          this.$store.commit(\"formConsulta/setCodigoEstado\", newValue);\n        }\n      },\n    },\n    incoterm: {\n      get() {\n        return this.$store.state.formConsulta.incoterm;\n      },\n      set(newValue) {\n        if (newValue != this.incoterm) {\n          this.$store.commit(\"formConsulta/setIncoterm\", newValue);\n        }\n      },\n    },\n  },\n  methods: {\n    inputNumeroNegativo(value) {\n      if (value < 0) value = 0;\n    },\n    validateNumber: (event) => {\n      let keyCode = event.keyCode;\n      if (keyCode < 48 || keyCode > 57) {\n        event.preventDefault();\n      }\n    },\n    verificaFormularioDadosGerais() {\n      this.$v.$touch();\n      if (!this.$v.$invalid) {\n        return true;\n      }\n      return false;\n    },\n    uppercase() {\n      this.formConsulta.numero_declaracao =\n        this.formConsulta.numero_declaracao.toUpperCase();\n    },\n    getMask() {\n      let numero_declaracao = this.formConsulta.numero_declaracao;\n      if (numero_declaracao == null || numero_declaracao == \"\") {\n        return false;\n      }\n      if (numero_declaracao.length <= 2) {\n        return \"###\";\n      } else if (numero_declaracao.length >= 3) {\n        let terceira_letra = numero_declaracao.slice(2, 3);\n        if (terceira_letra.match(/[a-z]/i) !== null) {\n          return \"##AA##########-#\";\n        } else {\n          return \"##/#######-#\";\n        }\n      }\n    },\n\n    verificaPreenchimento() {\n      this.$v.$touch();\n      if (!this.$v.$invalid) {\n        return true;\n      }\n      return false;\n    },\n  },\n};\n</script>\n<style lang=\"scss\">\n.centered-input input {\n  text-align: center !important;\n}\n.v-text-field.centered-input .v-label {\n  left: 50% !important;\n  font-size: 18px !important;\n  transform: translateX(-50%);\n  &.v-label--active {\n    transform: translateY(-18px) scale(0.75) translateX(-50%);\n  }\n}\n\n.incoterm.v-menu__content.v-menu__content--fixed {\n  max-height: 216px !important;\n  background-color: #ffffff;\n}\n\ninput[ID=\"date\"]::-webkit-input-placeholder {\n  visibility: hidden !important;\n}\n.v-text-field .v-icon {\n  font-size: 16px !important;\n}\n.v-tooltip__content {\n  height: fit-content !important;\n}\n</style>"]}]}