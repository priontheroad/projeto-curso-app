an class=\"mx-4 pipe-format\">|</span>\n        <span class=\"informativos-tela mx-2\">{{ paginationNotasFiscais.totalRegister }} exportador{{ paginationNotasFiscais.totalRegister > 1 ? 'es' : '' }}</span>\n      </div>\n\n      <div class=\"d-flex\">\n        <label class=\"mr-4 label-exibicao\">Exibição Completa</label>\n        <v-switch v-model=\"switch_notas\" inset color=\"#007CB3\" label=\"Resumo DANFE\" hide-details=\"\" />\n      </div>\n\n      <div>\n        <span class=\"text-caption font-weight-bold mr-3\">Páginas</span>\n        <span class=\"informativos-tela\">{{ paginationNotasFiscais.current }}</span>\n        <span class=\"mx-2\">/</span>\n        <span class=\"informativos-tela\">{{ paginationNotasFiscais.totalPage }}</span>\n        <span class=\"mx-4 pipe-format\">|</span>\n        <v-btn\n          @click=\"paginaAnterior\"\n          :disabled=\"loading == true || paginationNotasFiscais.current == 1\"\n          small\n          icon\n          class=\"text-capitalize mr-2\"\n          color=\"primary\"\n        >\n          <v-icon small>mdi-arrow-left</v-icon>\n        </v-btn>\n        <v-btn\n          @click=\"proximaPagina\"\n          :disabled=\"loading == true || paginationNotasFiscais.current == paginationNotasFiscais.totalPage\"\n          small\n          icon\n          class=\"text-capitalize ml-2\"\n          color=\"primary\"\n        >\n          <v-icon small>mdi-arrow-right</v-icon>\n        </v-btn>\n      </div>\n    </div>\n\n    <div class=\"format-divider\"></div>\n    \n    <div v-for=\"item in notas_fiscais\" :key=\"'aa' + item.id\" class=\"mb-10\">\n      <v-row class=\"mt-4\">\n        <v-col cols=\"12\">\n          <span style=\"color: #2d5071; font-weight: 800\">#{{ item.id_gerado }}</span>\n          <span> {{ item.nome }} </span>\n          <p class=\"informativos-tela\">{{ item.pais }}</p>\n        </v-col>\n      </v-row>\n\n      <QuadroResumoDANFE class=\"slot-resumo\" v-if=\"switch_notas == true\" :item=\"item\" />\n      \n      <div v-else>\n        <v-row>\n          <v-col cols=\"6\">\n            <QuadroCustosNF class=\"pr-2\" :item=\"item\" />\n          </v-col>\n          <v-col cols=\"6\">\n            <QuadroTributosNF class=\"pl-2\" :item=\"item\" />\n          </v-col>\n        </v-row>\n      </div>\n    </div>\n\n    <v-divider style=\"margin-top: 70px\"></v-divider>\n\n    <div class=\"d-flex justify-space-between align-center mt-4\" ref=\"scrollRodape\" id=\"scrollRodape\">\n      <div>\n        <v-btn\n          @click=\"paginaAnterior\"\n          :disabled=\"loading == true || paginationNotasFiscais.current == 1\"\n          small\n          text\n          color=\"primary\"\n          class=\"text-capitalize\"\n        >\n          <v-icon left>mdi-arrow-left</v-icon>\n          Anterior\n        </v-btn>\n      </div>\n\n      <div>\n        <span class=\"text-caption font-weight-bold mr-3\">Páginas</span>\n        <span class=\"informativos-tela\">{{ paginationNotasFiscais.current }}</span>\n        <span class=\"mx-2\">/</span>\n        <span class=\"informativos-tela\">{{ paginationNotasFiscais.totalPage }}</span>\n      </div>\n\n      <div>\n        <v-btn\n          @click=\"proximaPagina\"\n          :disabled=\"loading == true || paginationNotasFiscais.current == paginationNotasFiscais.totalPage\"\n          small\n          text\n          color=\"primary\"\n          class=\"text-capitalize\"\n        >\n          Próximo\n          <v-icon right>mdi-arrow-right</v-icon>\n        </v-btn>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport { mapGetters } from \"vuex\";\nimport formataMoedaMixin from \"@/mixins/formataMoedaMixin\";\nimport QuadroCustosNF from \"@/components/QuadrosResultado/QuadroCustosNF\";\nimport QuadroTributosNF from \"@/components/QuadrosResultado/QuadroTributosNF\";\nimport QuadroResumoDANFE from \"@/components/QuadrosResultado/QuadroResumoDANFE\";\n\nexport default {\n  name: \"\",\n  data() {\n    return {\n      page: 1,\n      switch_notas: false,\n    };\n  },\n  components: {\n    QuadroCustosNF,\n    QuadroTributosNF,\n    QuadroResumoDANFE\n  },\n  mixins: [formataMoedaMixin],\n  computed: {\n    ...mapGetters(\"resultadosFinal\", {\n      resultado: \"getState\",\n      notas_fiscais: \"getNotasFiscais\",\n      paginationNotasFiscais: \"getPaginationNotasFiscais\",\n    }),\n    ...mapGetters(\"consulta\", {\n      consulta: \"getState\",\n    }),\n    loading: {\n      get() {\n        return this.$store.state.loading.loading;\n      },\n      set(newValue) {\n        if (newValue != this.loading) {\n          this.$store.commit(\"loading/setLoading\", newValue);\n        }\n      },\n    },\n  },\n  methods: {\n    paginaAnterior() {\n      this.loading = true;\n      this.paginationNotasFiscais.current =\n        this.paginationNotasFiscais.current - 1;\n      this.getNotasFiscais();\n    },\n    proximaPagina() {\n      this.loading = true;\n      this.paginationNotasFiscais.current =\n        this.paginationNotasFiscais.current + 1;\n      this.getNotasFiscais();\n    },\n    getNotasFiscais() {\n      let page = this.paginationNotasFiscais.current;\n      let id_declaracao = this.consulta.id;\n\n      this.$root.$api\n        .get(\"/resultado/notas_fiscais/\" + id_declaracao + \"/?page=\" + page)\n        .then((response) => {\n          if (response.status == \"200\") {\n            this.loading = false;\n            let notasFiscais = response;\n\n            this.resultado.notas_fiscais = notasFiscais.data.data;\n            this.resultado.paginationNotasFiscais = {\n              current: notasFiscais.data.current_page,\n              totalPage: notasFiscais.data.last_page,\n              totalRegister: notasFiscais.data.total,\n            };\n          } else {\n            this.loading = false;\n            Object.bus.$emit(\"structure\", {\n              type: \"error\",\n              message: \"Não foi possível carregar nfe(s).\",\n              timeout: 1000,\n            });\n          }\n        })\n        .catch(() => {\n          this.loading = false;\n          Object.bus.$emit(\"structure\", {\n            type: \"error\",\n            message: \"Não foi possível carregar nfe(s).\",\n            timeout: 1000,\n          });\n        });\n    },\n  },\n};\n</script>\n<style lang=\"scss\">\n.slot-resumo p {\n  padding: 0 !important;\n  margin: 0 !important;\n}\n.label-exibicao {\n  color: rgba(0, 0, 0, 0.6);\n}\n.format-divider {\n  border: 1px solid #c3cfd9;\n}\n</style>"]}]}