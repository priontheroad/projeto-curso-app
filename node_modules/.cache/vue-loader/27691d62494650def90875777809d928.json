list-item>\n            <v-list-item-content class=\"mx-3\">\n              <div class=\"d-inline-flex\">\n                <span class=\"text-caption mr-4\">Nº DI/DUIMP</span>\n                <span class=\"tamanho-descricao\">{{\n                  resultado.numero_declaracao\n                }}</span>\n              </div>\n              <div class=\"d-inline-flex justify-space-between\">\n                <div>\n                  <span class=\"text-caption mr-4\">Data Declaração</span>\n                  <span class=\"tamanho-descricao\">{{\n                    dateTimeComputed(resultado.data)\n                  }}</span>\n                </div>\n                <div>\n                  <span class=\"text-caption mr-4\">UF</span>\n                  <span class=\"tamanho-descricao\">{{\n                    resultado.codigo_estado\n                  }}</span>\n                </div>\n              </div>\n              <div class=\"d-inline-flex\">\n                <span class=\"text-caption mr-4\">Modal Operação</span>\n                <span class=\"tamanho-descricao\">\n                  {{ buscarTipoOperacao() }}\n                </span>\n              </div>\n              <div class=\"d-inline-flex\">\n                <span class=\"text-caption mr-4\">Total Adições</span>\n                <span class=\"tamanho-descricao\">\n                  {{ resultado.quantidade_total_adicoes }}\n                </span>\n              </div>\n            </v-list-item-content>\n          </v-list-item>\n          <v-divider></v-divider>\n\n          <!-- bloco 01.2 -->\n          <v-list-item class=\"mx-3 my-2\">\n            <v-list-item-content>\n              <span class=\"text-caption\">Valor da Mercadoria (VMLE)</span>\n              <v-row no-gutters>\n                <template v-if=\"resultado.valor_mercadoria.moeda == 'BRL'\">\n                  <v-col cols=\"5\" class=\"tamanho-descricao\">\n                    {{ vlrMoeda(resultado.valor_mercadoria.valor_real, \"BRL\") }}\n                  </v-col>\n                </template>\n                <template v-else>\n                  <v-col cols=\"5\" class=\"tamanho-descricao\">\n                    {{\n                      vlrCodigoMoeda(\n                        resultado.valor_mercadoria.valor_moeda,\n                        resultado.valor_mercadoria.moeda\n                      ) | truncate(20, \"...\")\n                    }}\n                  </v-col>\n                  <v-col cols=\"2\" class=\"d-flex justify-center\">\n                    <v-icon small>mdi-arrow-right</v-icon>\n                  </v-col>\n                  <v-col cols=\"5\" class=\"tamanho-descricao text-right\">\n                    {{\n                      vlrMoeda(resultado.valor_mercadoria.valor_real, \"BRL\")\n                        | truncate(20, \"...\")\n                    }}\n                  </v-col>\n                </template>\n              </v-row>\n\n              <span class=\"text-caption\">Frete Internacional</span>\n              <v-row no-gutters>\n                <template v-if=\"resultado.frete_internacional.moeda == 'BRL'\">\n                  <v-col cols=\"12\" class=\"tamanho-descricao\">\n                    {{\n                      vlrMoeda(resultado.frete_internacional.valor_real, \"BRL\")\n                    }}\n                  </v-col>\n                </template>\n                <template v-else>\n                  <v-col cols=\"5\" class=\"tamanho-descricao\">\n                    {{\n                      vlrCodigoMoeda(\n                        resultado.frete_internacional.valor_moeda,\n                        resultado.frete_internacional.moeda\n                      ) | truncate(20, \"...\")\n                    }}\n                  </v-col>\n                  <v-col cols=\"2\" class=\"d-flex justify-center\">\n                    <v-icon small>mdi-arrow-right</v-icon>\n                  </v-col>\n                  <v-col cols=\"5\" class=\"tamanho-descricao text-right\">\n                    {{\n                      vlrMoeda(resultado.frete_internacional.valor_real, \"BRL\")\n                        | truncate(20, \"...\")\n                    }}\n                  </v-col>\n                </template>\n              </v-row>\n\n              <span class=\"text-caption\">Seguro Internacional</span>\n              <v-row no-gutters>\n                <template v-if=\"resultado.seguro_internacional.moeda == 'BRL'\">\n                  <v-col cols=\"12\" class=\"tamanho-descricao\">\n                    {{\n                      vlrMoeda(resultado.seguro_internacional.valor_real, \"BRL\")\n                    }}\n                  </v-col>\n                </template>\n                <template v-else>\n                  <v-col cols=\"5\" class=\"tamanho-descricao\">\n                    {{\n                      vlrCodigoMoeda(\n                        resultado.seguro_internacional.valor_moeda,\n                        resultado.seguro_internacional.moeda\n                      ) | truncate(20, \"...\")\n                    }}\n                  </v-col>\n                  <v-col cols=\"2\" class=\"d-flex justify-center\">\n                    <v-icon small>mdi-arrow-right</v-icon>\n                  </v-col>\n                  <v-col cols=\"5\" class=\"tamanho-descricao text-right\">\n                    {{\n                      vlrMoeda(resultado.seguro_internacional.valor_real, \"BRL\")\n                        | truncate(20, \"...\")\n                    }}\n                  </v-col>\n                </template>\n              </v-row>\n            </v-list-item-content>\n          </v-list-item>\n          <v-divider class=\"ml-6\"></v-divider>\n          <v-list-item class=\"ml-2\">\n            <v-list-item-content>\n              <v-expansion-panels flat>\n                <v-expansion-panel style=\"background: #ebf5fa\">\n                  <v-expansion-panel-header>\n                    <div class=\"d-flex justify-space-between\">\n                      <div>\n                        <div>\n                          <span class=\"label-data-cotacao\"\n                            >Cotação utilizada\n                            {{\n                              datePtBR(resultado.cotacoes[0].data_cotacao)\n                            }}</span\n                          >\n                        </div>\n                      </div>\n                      <div class=\"label-ver-cotacao\">Ver cotação</div>\n                    </div>\n                  </v-expansion-panel-header>\n                  <v-divider></v-divider>\n                  <v-expansion-panel-content>\n                    <v-row>\n                      <v-col\n                        cols=\"4\"\n                        v-for=\"item in resultado.cotacoes\"\n                        :key=\"item.tipo_moeda\"\n                      >\n                        <p\n                          class=\"\n                            text-caption\n                            color-button-slider\n                            mt-5\n                            format-slider\n                          \"\n                        >\n                          {{ item.tipo_moeda }}\n                        </p>\n                        <p class=\"format-slider formata-resultados\">\n                          BRL {{ moedaSemArredondamento(item.cotacao) }}\n                        </p>\n                      </v-col>\n                    </v-row>\n                    <v-row>\n                      <v-col>\n                        <span class=\"label-data-cotacao\">\n                          Cotação referente ao fechamento do último dia útil\n                          anterior a data da DI.</span\n                        >\n                      </v-col>\n                    </v-row>\n                  </v-expansion-panel-content>\n                </v-expansion-panel>\n              </v-expansion-panels>\n            </v-list-item-content>\n          </v-list-item>\n\n          <v-divider class=\"ml-6\"></v-divider>\n\n          <v-list-item>\n            <v-list-item-content class=\"mx-3\">\n              <v-row no-gutters>\n                <v-col class=\"text-caption mr-4\">Subtotal CIF</v-col>\n                <v-col class=\"tamanho-descricao text-right\">\n                  {{ vlrMoeda(resultado.subtotal_cif, \"BRL\") }}\n                </v-col>\n              </v-row>\n            </v-list-item-content>\n          </v-list-item>\n\n          <v-divider class=\"ml-6\"></v-divider>\n\n          <!-- <v-list-item v-if=\"resultado.capatazia.valor_real\"> -->\n          <v-list-item v-if=\"buscarTipoOperacao() == 'Marítima'\">\n            <v-list-item-content class=\"mx-3\">\n              <v-row no-gutters>\n                <v-col class=\"text-caption mr-4\">Capatazia/THC</v-col>\n                <v-col class=\"tamanho-descricao text-right\">\n                  {{ vlrMoeda(resultado.capatazia.valor_real, \"BRL\") }}\n                </v-col>\n              </v-row>\n            </v-list-item-content>\n          </v-list-item>\n\n          <v-divider\n            v-if=\"buscarTipoOperacao() == 'Marítima'\"\n            class=\"ml-6\"\n          ></v-divider>\n\n          <v-list-item>\n            <v-list-item-content class=\"mx-3\">\n              <v-row no-gutters>\n                <v-col class=\"text-caption mr-4\">Valor Aduaneiro (VMLD)</v-col>\n                <v-col class=\"tamanho-descricao text-right formata-resultados\">\n                  {{ vlrMoeda(resultado.valor_aduaneiro, \"BRL\") }}\n                </v-col>\n              </v-row>\n            </v-list-item-content>\n          </v-list-item>\n          l\n          <v-list-item v-if=\"buscarTipoOperacao() == 'Marítima'\">\n            <v-list-item-content class=\"mx-3\">\n              <v-row no-gutters>\n                <v-col class=\"text-caption mr-4\">AFRMM</v-col>\n                <v-col class=\"formata-outros text-right\"\n                  >{{ vlrMoeda(resultado.afrmm.valor_real, \"BRL\") }}\n                </v-col>\n              </v-row>\n            </v-list-item-content>\n          </v-list-item>\n\n          <v-list-item>\n            <v-list-item-content class=\"mx-3\">\n              <v-row no-gutters>\n                <v-col class=\"text-caption mr-4\">Outras Despesas</v-col>\n                <v-col class=\"text-right formata-outros\">{{\n                  vlrMoeda(resultado.outras_despesas.valor_real, \"BRL\")\n                }}</v-col>\n              </v-row>\n            </v-list-item-content>\n          </v-list-item>\n\n          <v-list-item>\n            <v-list-item-content class=\"mx-3\">\n              <v-row no-gutters>\n                <v-col class=\"text-caption mr-4\">Taxa Siscomex</v-col>\n                <v-col class=\"formata-resultados text-right\">{{\n                  vlrMoeda(resultado.taxa_siscomex, \"BRL\")\n                }}</v-col>\n              </v-row>\n            </v-list-item-content>\n          </v-list-item>\n        </template>\n      </v-list>\n    </div>\n  </div>\n</template>\n<script>\nimport { mapGetters } from \"vuex\";\nimport formataDataMixin from \"@/mixins/formataDataMixin\";\nimport formataMoedaMixin from \"@/mixins/formataMoedaMixin\";\n\nexport default {\n  name: \"DetalheConsulta\",\n  data() {\n    return {\n      model: null,\n      cotacoes: [],\n    };\n  },\n  mixins: [formataDataMixin, formataMoedaMixin],\n  filters: {\n    truncate: function (text, length, suffix) {\n      if (\n        typeof text !== \"undefined\" &&\n        text !== null &&\n        text !== \"\" &&\n        text.length > length\n      ) {\n        return text.substring(0, length) + suffix;\n      } else {\n        return text;\n      }\n    },\n  },\n  computed: {\n    ...mapGetters(\"informacoesPadrao\", {\n      estados: \"getEstados\",\n      operacoes: \"getTipoOperacao\",\n    }),\n    ...mapGetters(\"resultadoTelaSimples\", {\n      resultado: \"getState\",\n    }),\n    ...mapGetters(\"consulta\", {\n      consulta: \"getState\",\n    }),\n  },\n  methods: {\n    dateTimeComputed(data) {\n      if (data == null || data == \"\") {\n        return \"\";\n      }\n      let arDateTime = data.split(\" \");\n      return this.datePtBR(arDateTime[0]);\n    },\n\n    buscarTipoOperacao() {\n      let tipo_operacao = this.resultado.tipo_operacao;\n      let operacao = \"\";\n      if (tipo_operacao !== \"\") {\n        this.operacoes.forEach(\n          (element) => {\n            if (element.id == tipo_operacao) {\n              operacao = element.descricao;\n            }\n          },\n          tipo_operacao,\n          operacao\n        );\n      }\n\n      return operacao;\n    },\n  },\n};\n</script>\n<style lang=\"scss\">\n.min-button::before {\n  display: none;\n}\n.style-width-list {\n  max-width: 95% !important;\n}\n.label-ver-cotacao {\n  font-size: 12px;\n  font-weight: bold;\n  color: #2d5071;\n}\n.label-data-cotacao {\n  font-size: 12px;\n}\n.v-expansion-panel::before {\n  box-shadow: none;\n}\n</style>\n"]}]}