\n  <div class=\"header\">\n    <v-app-bar app flat class=\"app-bar-style\">\n      <div\n        class=\"d-flex align-center\"\n        style=\"cursor: pointer\"\n        @click=\"voltarHome()\"\n      >\n        <img\n          alt=\"Rateio Comex\"\n          style=\"width: 190px; height: 32px\"\n          src=\"@/assets/img/logo_rateio.svg\"\n          width=\"auto\" height=\"auto\" \n        />\n      </div>\n      <div class=\"ml-6 mt-1\">\n        <svg width=\"4\" height=\"18\" viewBox=\"0 0 7 24\">\n          <path\n            id=\"chevron\"\n            d=\"M65,43l6.458-12.047L65,19.514\"\n            transform=\"translate(-64.559 -19.269)\"\n            fill=\"none\"\n            stroke=\"#C3CFD9\"\n            stroke-width=\"2\"\n          />\n        </svg>\n      </div>\n      <v-breadcrumbs :items=\"items\">\n        <template v-slot:divider>\n          <svg width=\"4\" height=\"18\" viewBox=\"0 0 7 24\">\n            <path\n              id=\"chevron\"\n              d=\"M65,43l6.458-12.047L65,19.514\"\n              transform=\"translate(-64.559 -19.269)\"\n              fill=\"none\"\n              stroke=\"#C3CFD9\"\n              stroke-width=\"2\"\n            />\n          </svg>\n        </template>\n        <template v-slot:item=\"{ item }\">\n          <v-breadcrumbs-item :to=\"item.href\" :disabled=\"item.disabled\">\n            {{ item.text }}\n          </v-breadcrumbs-item>\n        </template>\n      </v-breadcrumbs>\n      <v-spacer />\n      <!--\n      <span class=\"#2d5071-text\">\n        <v-btn\n          @click.stop=\"validarAberturaModalConsulta()\"\n          color=\"#2d5071\"\n          class=\"elevation-0 button-nova-consulta\"\n        >\n          <span>Criar Novo</span>\n          <v-icon right>mdi-plus-circle-outline</v-icon>\n        </v-btn>\n      </span>\n      -->\n      <v-toolbar-items class=\"ml-2\">\n        <v-btn icon color=\"#2d5071\" @click=\"voltarHome()\">\n          <v-icon>mdi-home-circle-outline</v-icon>\n        </v-btn>\n        <v-btn icon color=\"#2d5071\" @click=\"direcionarAjuda()\">\n          <v-icon>mdi-help-circle-outline</v-icon>\n        </v-btn>\n        <!-- menu usuário -->\n        <v-menu offset-y class=\"formata-menu\">\n          <template v-slot:activator=\"{ on, attrs }\">\n            <v-btn icon color=\"#2d5071\" v-bind=\"attrs\" v-on=\"on\">\n              <v-avatar color=\"white\" size=\"34\">\n                <v-icon small color=\"#2d5071\">mdi-account-outline</v-icon>\n              </v-avatar>\n            </v-btn>\n          </template>\n          <v-list nav dense>\n            <v-list-item>\n              <v-avatar class=\"mr-3\" color=\"white\" size=\"34\">\n                <v-icon class=\"icon-user\" color=\"#2d5071\">mdi-account</v-icon>\n              </v-avatar>\n              <v-list-item-title class=\"font-weight-bold\">\n                {{ nome_usuario }}\n              </v-list-item-title>\n            </v-list-item>\n            <v-divider class=\"my-2\"></v-divider>\n            <v-list-item-group color=\"#2d5071\">\n              <v-list-item\n                href=\"http://www.econeteditora.com.br/\"\n                target=\"_blank\"\n              >\n                <v-list-item-icon class=\"mr-4\">\n                  <v-icon color=\"#2d5071\">mdi-arrow-left</v-icon>\n                </v-list-item-icon>\n                <v-list-item-title\n                  >Voltar para Econet Editora</v-list-item-title\n                >\n              </v-list-item>\n              <v-list-item href=\"http://www.econeteditora.com.br/\">\n                <v-list-item-icon class=\"mr-4\">\n                  <v-icon color=\"error\">mdi-power</v-icon>\n                </v-list-item-icon>\n                <v-list-item-title>Encerrar Sessão</v-list-item-title>\n              </v-list-item>\n            </v-list-item-group>\n          </v-list>\n        </v-menu>\n      </v-toolbar-items>\n    </v-app-bar>\n  </div>\n</template>\n<script>\nimport { mapGetters } from \"vuex\";\nimport Form from \"@/services/modules/form\";\n\nexport default {\n  name: \"Topo\",\n \n  data() {\n    return {\n    };\n  },\n\n  computed: {\n    ...mapGetters(\"auth\", {\n      nome_usuario: \"getNomeUsuario\",\n      id_usuario: \"getIdUsuario\",\n    }),\n    modalTermo: {\n      get() {\n        return this.$store.state.termo.modalTermo;\n      },\n      set(newValue) {\n        if (newValue != this.modalTermo) {\n          this.$store.commit(\"termo/setModalTermo\", newValue);\n        }\n      },\n    },\n    snackbarTermo: {\n      get() {\n        return this.$store.state.termo.snackbarTermo;\n      },\n      set(newValue) {\n        if (newValue != this.snackbarTermo) {\n          this.$store.commit(\"termo/setsnackbarTermo\", newValue);\n        }\n      },\n    },\n    items: {\n      get() {\n        return this.$store.state.breadcrumbs.crumb;\n      },\n      set(newValue) {\n        if (newValue != this.items) {\n          this.$store.commit(\"breadcrumbs/setCrumb\", newValue);\n        }\n      },\n    },\n    handleNovaSimulacao: {\n      get() {\n        return this.$store.state.modalNovaSimulacao.modalNovaSimulacao;\n      },\n      set(newValue) {\n        if (newValue != this.handleNovaSimulacao) {\n          this.$store.commit(\"modalNovaSimulacao/setModalNovaSimulacao\", newValue);\n        }\n      },\n    },\n    ...mapGetters(\"informacoesPadrao\", {\n      status_carregamento: \"getStatusCarregamento\"\n    }),\n  },\n  methods: {\n    voltarHome() {\n      if(this.$router.currentRoute.path !== \"/\") {\n        this.$router.push(\"/\");\n      }\n    },\n    direcionarAjuda() {\n      this.$router.push(\"/ajuda\");\n    },\n    validarAberturaModalConsulta(){\n      //verificar se informacoesPadroes carregaram\n      if(this.status_carregamento == false){\n        Object.bus.$emit(\"structure\", {\n          message: \"Erro ao carregar informações, entre em contato com a econet.\",\n          type: \"error\",\n          timeout: 10000,\n        });\n        this.handleNovaSimulacao = false;\n        return false;\n      }\n\n      if(localStorage.getItem(\"aceite_termo\")){\n        let form = new Form();\n        form.carregarNovaConsulta();\n        this.handleNovaSimulacao = true;\n        this.snackbarTermo = false;\n      }else{\n        this.handleNovaSimulacao = false;\n        this.modalTermo = true;\n        this.snackbarTermo = true;\n      }\n    }\n  },\n};\n</script>\n<style lang=\"scss\" scoped>\n.icon-user {\n  border: 2px solid #2d5071 !important;\n}\n.button-nova-consulta {\n  text-transform: none;\n  color: white !important;\n}\n.link-home {\n  cursor: pointer;\n}\n.spacing {\n  line-height: 1em;\n}\n\n.header .econet {\n  font-weight: bold;\n  color: #2d5071;\n  font-size: 0.6em;\n}\n.header .titulo {\n  letter-spacing: 1px;\n  color: #718aa0;\n}\n.app-bar-style {\n  border-bottom: 1px solid rgba(130, 175, 217, 0.25) !important;\n}\n\nheader,\n.v-app-bar {\n  font-weight: 300 !important;\n  background-color: rgb(228, 237, 247) !important;\n  opacity: 1 !important;\n  position: relative;\n}\n\n@supports ((-webkit-backdrop-filter: none) or (backdrop-filter: none)) {\n  .header .v-app-bar {\n    background-color: rgba(212, 229, 250, 0.4) !important;\n    -webkit-backdrop-filter: blur(10px) !important;\n    backdrop-filter: blur(10px) !important;\n  }\n  .v-toolbar__content {\n    background-color: #2d5071;\n  }\n}\n.backdrop-blur {\n  background-color: rgba(255, 255, 255, 0.9);\n}\n\n.user-text {\n  text-transform: none;\n  color: #2d5071;\n  font-weight: 700;\n  font-size: 14px;\n  letter-spacing: 0 !important;\n}\n\n.logo-header {\n  height: 5.5vh;\n  width: 4vw;\n}\n.v-menu__content.v-menu__content--fixed {\n  max-height: 161px !important;\n  background-color: #FFFFFF;\n}\n</style>"]}]}